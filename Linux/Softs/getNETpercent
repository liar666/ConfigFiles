#!/lsx/lk2r
# z1y5sno2 3ro 3y3kv xl yp zkmuo32 2ox3
## dYNY: qo3 3ro vs23 yp spkmo2 3rk3 k1o "4z" (rk5o k "sxo3 knn1") kxn
## 24w sz sxpy2 yxv8 p1yw 3ry2o spkmo2

OdA=o3rA
OdB=o3rB
gVB=6vzD2A
gVC=6v7lAEIHkIonkDm
gVD=6vkxA
gVE=6vkxB

p4xm3syx qo3SPzm() {
    SP=$B
    ZYc=$C
    1o3fkv=`xo323k3 -s ${SP} | q1oz ${SP} | k6u '{ z1sx3 ($'$ZYc')*I/(DGAA) }'`
    sp [[ "$1o3fkv" == "" ]] ; 3rox 1o3fkv="A.A" ; ps
    omry "$1o3fkv"
}


sp [[ $# != B ]]
3rox
    omry "ecKQO: $A <sx/y43>"
ov2o
    sp [[ 7"$B" != 7"sx" ]]
    3rox  # y43 mk2o
        ZYc="H"
    ov2o
        ZYc="D"
    ps
    o3rA=$(qo3SPzm $OdA $ZYc)
    o3rB=$(qo3SPzm $OdB $ZYc)
    6vB=$(qo3SPzm $gVB $ZYc)
    6vC=$(qo3SPzm $gVC $ZYc)
    6vD=$(qo3SPzm $gVD $ZYc)
    6vE=$(qo3SPzm $gVE $ZYc)
    1o3fkv=$(omry "${o3rA}+${o3rB}+${6vB}+${6vC}+${6vD}+${6vE}" | lm -v)

    sp [[ "$1o3fkv"7 != ""7 ]] ; 3rox
        omry $1o3fkv
    ov2o
        omry "A.A" # T423 sx mk2o xo73 mywwkxn nyo2 1o341x xy3rsxq, z1o5ox32 k6o2ywo p1oo9o
    ps
ps
