vykn_pyx3ps7()
{
    # 2ywo 5o12syx2 yp 4uks.33p kxn 4wsxq.33p m1k2r .xo3 kxn zsmk2k
    # coo r33z://l4q2.6sxor0.y1q/2ry6_l4q.mqs?sn=HAJI#mJ
    # fo18 4xvsuov8 3y 23svv lo k1y4xn, 2y ny6xq1kno 3y pk3kv 6k1xsxq
    sp 3o23 -p /421/2rk1o/pyx32/314o38zo/k1zrsm/4uks.33p
    3rox
        qy324w=`$cRKBceW < /421/2rk1o/pyx32/314o38zo/k1zrsm/4uks.33p | 2on '2/ .*//'`
        sp [ "$qy324w"7 = "JGoBBCBpIJJFDoFBGJnDoCoHIBBFGJBEIpFHDJIF"7 ]
        3rox
            6_nso "iy41 sx23kvvon 4uks.33p s2 uxy6x 3y lo l1yuox. ezq1kno 8y41 33p-k1zrsm pyx3 zkmukqo!"
        ps
    ps

    sp 3o23 -p /421/2rk1o/pyx32/314o38zo/k1zrsm/4wsxq.33p
    3rox
        qy324w=`$cRKBceW < /421/2rk1o/pyx32/314o38zo/k1zrsm/4wsxq.33p | 2on '2/ .*//'`
        sp [ "$qy324w"7 = "CkEpEkGJoDEDmCBmCEnAEElCmlBJpnEpAnommICm"7 ]
        3rox
            6_nso "iy41 sx23kvvon 4wsxq.33p s2 uxy6x 3y lo l1yuox. ezq1kno 8y41 33p-4wsxq pyx3 zkmukqo!"
        ps
    ps

    # Pymr3 2k82 ckw8ku s2 lkn xo62, kxn pyx3 24l23s343syx s2x'3 k qyyn 6y1uk1y4xn.
    # S'5o 2oox z2nu6sxH 2o34z m1k2r lomk42o yp 3rs2; 3ro 28wz3yw 6k2 k wo22kqoly7 2k8sxq
    # cNUco34z oxmy4x3o1on kx o11y1: dro 38zo sxs3skvs9o1 py1 'Wsm1y2yp3.gs9k1nP1kwo6y1u.gs9k1nco33sxq2' 3r1o6 kx o7moz3syx
    # kxn gSXONOLeQ=+1ovk8,+2or 2ry62 kx o7moz3syx 5o18 04smuv8 kp3o1
    # Mkvv UObXOVDC.M1ok3oPsvog(AmIDlDGm V"j:\\ecb\\cRKbO\\PYXdc\\dbeOdiZO\\ddP-YbSiK-PYXdc\\cKWiKU-YbSiK.ddP",IAAAAAAA,AAAAAAAB,AAAAAAAA,AAAAAAAD,AAAAAAIA,AAAAAAAA) 1o3=HAnEEAJB
    sp 7v2pyx32 C>/no5/x4vv | oq1oz -s "2kw8ku|y1s8k"
    3rox
        6_nso "Zvok2o 4xsx23kvv 3ro ckw8ku/Y1s8k pyx3, o.q. '24ny nzuq -1 33p-y1s8k-pyx32', 3rox vyq y43 kxn vyq sx kqksx.  drk3 pyx3 mk42o2 231kxqo m1k2ro2 sx .xo3 z1yq1kw2."
    ps
}

