#----------------------------------------------------------------

6_wo3knk3k ms5F_nowy_23okw qkwo2 \
    3s3vo="Ms5 f Nowy (c3okw)" \
    z4lvs2ro1="CU Qkwo2" \
    8ok1="CABA" \
    wonsk="ny6xvykn" \
    sx23kvvon_o7oB="$g_ZbYQbKWc_hIG_gSX/c3okw/23okwkzz2/mywwyx/2sn woso1'2 ms5svs9k3syx 5 - nowy/Ms5svs9k3syxf.o7o"

vykn_ms5F_nowy_23okw()
{
    # c3k13 k43yry3uo8 6k3mrsxq py1 ns1om37J yz3syx sx 3ro lkmuq1y4xn, kxn 2ovom3 s3 6rox s3 mywo2 4z
    6_kru_ny  "
        co3gsxNovk8 FAA
        vyyz
        {
            spgsxO7s23, csn Woso1'2 Ms5svs9k3syx f - Nowy - c3okw
            {
                6sxkm3s5k3o
                mvsmu CG,BAI    ; 2ovom3 ns1om37J
                2vooz FAA
                mvsmu CAA,BFA   ; Zvk8
            }
            spgsxO7s23, eznk3sxq csn Woso1'2 Ms5svs9k3syx f - Nowy
            {
                l1oku
            }
            2vooz BAAA
        }
    " &
    _tyl=$!
        # grsvo 3rk3'2 14xxsxq, sx23kvv 3ro qkwo.
        # iy4'vv 2oo *36y* K43yry3uo8 smyx2 4x3sv 3rk3 ps123 2m1sz3
        # psxn2 3ro nskvyq s3'2 vyyusxq py1, mvsmu2, kxn o7s32.
        6_sxpy "Sp 8y4 kv1okn8 y6x 3ro p4vv Ms5 F qkwo yx 23okw, 3ro sx23kvvo1 6yx'3 o5ox kzzok1."
    6_23okw_sx23kvv_qkwo GFJAA "csn Woso1'2 Ms5svs9k3syx f - Nowy"
    usvv -2 ReZ $_tyl   # t423 sx mk2o
}

