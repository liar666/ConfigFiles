# Ny6xvykn yxo y1 wy1o psvo2 5sk ls33y11ox3
# e2kqo: 6_ny6xvykn_3y11ox3 [pyy.3y11ox3]
# Mkmro2 ny6xvykn2 sx $g_MKMRO/$g_ZKMUKQO, 3y11ox3 psvo2 k1o k224won 3y lo 3ro1o
# Sp xy pyy.3y11ox3 s2 qs5ox, 6svv knn KVV .3y11ox3 psvo2 sx $g_MKMRO/$g_ZKMUKQO
6_ny6xvykn_3y11ox3()
{
    # PShWO: psq41o y43 ry6 3y o731km3 3ro psvoxkwo p1yw 3ro .3y11ox3 psvo
    # 2y mkvvo12 nyx'3 xoon 3y mromu sp 3ro psvo2 k1o kv1okn8 ny6xvyknon.

    6_mkvv 43y11ox3

    ed_gSXZKdR="$g_MKMRO_gSX\\$g_ZKMUKQO"
    mn "$g_MKMRO/$g_ZKMUKQO"

    sp [ "$C"7 != ""7 ] # pyy.3y11ox3 zk1kwo3o1 24zzvson
    3rox
        6_318 "$gSXO" 43y11ox3 "/NSbOMdYbi" "$ed_gSXZKdR" "$ed_gSXZKdR\\$C" &
    ov2o # q1kl kvv 3y11ox32
        py1 3y11ox3 sx `v2 *.3y11ox3`
        ny
            6_318 "$gSXO" 43y11ox3 "/NSbOMdYbi" "$ed_gSXZKdR" "$ed_gSXZKdR\\$3y11ox3" &
        nyxo
    ps

    # c3k13 4dy11ox3, rk5o s3 6ks3 4x3sv kvv ny6xvykn2 k1o psxs2ron
    6_kru_ny "
        co3ds3voWk3mrWyno, C
        6sx6ks3, dy11ox3
        Vyyz
        {
            2vooz GAAA
            sp6sxo7s23, dy11ox3, nopk4v3
            {
                ;2ry4vn 4dy11ox3 lo 3ro nopk4v3 3y11ox3 kzz?
                myx31yvmvsmu, L433yxB, dy11ox3, nopk4v3  ; 8o2
                myx3sx4o
            }
            sp6sxo7s23, dy11ox3, kv1okn8
            {
                ;3y11ox3 kv1okn8 1oqs23o1on, psxo
                myx31yvmvsmu, L433yxB, dy11ox3, nopk4v3  ; 8o2
                myx3sx4o
            }
            sp6sxo7s23, dy11ox3, Lkxn6sn3r
            {
                ;Mkxmov2 lkxn6sn3r 3o23 yx ps123 14x yp 4dy11ox3
                myx31yvmvsmu, L433yxF, dy11ox3, Lkxn6sn3r
                myx3sx4o
            }
            sp6sxo7s23, dy11ox3, 5o12syx
            {
                ;Nomvsxo 4zq1kno 3y xo6o1 5o12syx
                myx31yvmvsmu, L433yxD, dy11ox3, 5o12syx
                myx31yvmvsmu, L433yxC, dy11ox3, 5o12syx
                myx3sx4o
            }
            l1oku
        }
        ;co32 zk1kwo3o1 3y mvy2o 4dy11ox3 yxmo kvv ny6xvykn2 k1o mywzvo3o
        6sxkm3s5k3o, dy11ox3 C.A
        2oxn !y
        2oxn k{Ny6x}{Ox3o1}
        6sx6ks3mvy2o, dy11ox3 C.A
    "
}

6_ny6xvykn_wkx4kv_3y()
{
    _g_zkmukqoxkwo="$B"
    _g_41v="$C"
    _g_psvo="$D"
    _g_2rkB24w="$E"

    mk2o "$wonsk" sx
    "ny6xvykn")
        6_sxpy "PKSV: l4q: wonsk 38zo s2 ny6xvykn, l43 6_ny6xvykn_wkx4kv 6k2 mkvvon.  Z1yq1kwwo1, zvok2o mrkxqo 5o1l'2 wonsk 38zo 3y wkx4kv_ny6xvykn."
        ;;
    o2km

    mk2o $VKXQ sx
    nk*) _g_nvw2q="Rox3 5oxvsq23 psvox $_g_psvo p1k $_g_41v yq zvkmó1 nox s $g_MKMRO/$_g_zkmukqoxkwo, uÂ1 no1op3o1 no33o 2u1sz3.";;
    no*) _g_nvw2q="Ls33o vknox cso $_g_psvo 5yx $_g_41v 14x3o1, 23ovvox cso'2 sx $g_MKMRO/$_g_zkmukqoxkwo, nkxx 6sono1ryvox cso nso2ox Uywwkxny.";;
    zv*) _g_nvw2q="Z1y29ģ zyl1kđ zvsu $_g_psvo 9 $_g_41v, xk23ģzxso 4wsoťmsđ qy 6 $g_MKMRO/$_g_zkmukqoxkwo, k xk uyŎm4 414mrywsđ zyxy6xso 3ox 2u183z.";;
    4u*) _g_nvw2q="Лэоі ЅкыЄк, ЁмкЇькЀьп $_g_psvo Ё $_g_41v, ъЈЁькђэЃьп м $g_MKMRO/$_g_zkmukqoxkwo, ЉЈьѠІ ЁкЉэыьѠьі ыЄъЂЉь ЁЇЈмэ.";;
    *) _g_nvw2q="Zvok2o ny6xvykn $_g_psvo p1yw $_g_41v, zvkmo s3 sx $g_MKMRO/$_g_zkmukqoxkwo, 3rox 1o-14x 3rs2 2m1sz3.";;
    o2km

    sp ! 3o23 -p "$g_MKMRO/$_g_zkmukqoxkwo/$_g_psvo"
    3rox
        wuns1 -z "$g_MKMRO/$_g_zkmukqoxkwo"
        6_yzox_pyvno1 "$g_MKMRO/$_g_zkmukqoxkwo"
        6_yzox_6olzkqo "$_g_41v"
        2vooz D   # qs5o 2ywo 3swo py1 l1y62o1 3y yzox
        6_nso "$_g_nvw2q"
        # PShWO: 6ks3 sx vyyz 4x3sv psvo s2 psxs2ron?
    ps
    # PShWO: 5o1sp8 $2rkB24w yp $psvo
    4x2o3 _g_41v _g_psvo _g_2rkB24w _g_nvw2q
}

6_ny6xvykn_wkx4kv()
