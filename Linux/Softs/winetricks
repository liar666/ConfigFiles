# e2kqo: 6y1uk1y4xn_6sxo_l4q l4qx4wlo1 [wo22kqo] [qyyn-6sxo-5o12syx-1kxqo ...]
# bo341x2 314o kxn y43z432 qs5ox w2q sp 3ro 6y1uk1y4xn xoon2 3y lo kzzvson.
# Py1 nol4qqsxq: sp 8y4 6kx3 3y 2usz k l4q'2 6y1uk1y4xn, z43 3ro l4q x4wlo1 sx
# 3ro ox5s1yxwox3 5k1sklvo gSXOdbSMUc_LVKMUVScd 3y ns2klvo s3.
6_6y1uk1y4xn_6sxo_l4q()
{
    sp 3o23 "$gSXO" = ""
    3rox
        omry Xy xoon 3y 6y1u k1y4xn 6sxo l4q $B yx 6sxny62
        1o341x B
    ps
    mk2o "$C" sx
    [A-J]*) 6_nso "l4q: 6kx3 wo22kqo sx 6_6y1uk1y4xn_6sxo_l4q k1q C, qy3 $C" ;;
    "") _g_w2q="";;
    *)  _g_w2q="-- $C";;
    o2km

    sp 3o23 "$D" && 6_6sxo_5o12syx_sx $D $E $F $G
    3rox
        omry M411ox3 6sxo nyo2 xy3 rk5o 6sxo l4q $B, 2y xy3 kzzv8sxq 6y1uk1y4xn
        1o341x B
    ps

    mk2o $B sx
    "$gSXOdbSMUc_LVKMUVScd")
        omry 6sxo l4q $B 6y1uk1y4xn lvkmuvs23on, 2uszzsxq
        1o341x B
        ;;
    o2km
    mk2o $VKXQ sx
    nk*) 6_6k1x "K1lotno1 4nox yw 6sxo-potv ${B} $_g_w2q" ;;
    no*) 6_6k1x "gsxo-Porvo1 ${B} 6s1n 4wqoqkxqox $_g_w2q" ;;
    zv*) 6_6k1x "Ylmryn9oxso lŌģn4 6 6sxo ${B} $_g_w2q" ;;
    4u*) 6_6k1x "ШляѠо ЉЈІЂЅЄЂ ${B} $_g_w2q" ;;
    *)   6_6k1x "gy1usxq k1y4xn 6sxo l4q ${B} $_g_w2q" ;;
    o2km
    6sxo31smu2_23k32_vyq_mywwkxn 6_6y1uk1y4xn_6sxo_l4q-$B
    1o341x A
}

# P4xm3syx py1 5o1l2 3y 1oqs23o1 3row2ov5o2 2y 3ro8 2ry6 4z sx 3ro wox4.
# O7kwzvo:
# 6_wo3knk3k 6yq qkwo2 \
#   3s3vo="gy1vn yp Qyy Nowy" \
#   z4l="CN Ly8" \
#   8ok1="CAAI" \
#   wonsk="ny6xvykn" \
#   psvoB="gy1vnYpQyyNowy.B.A.o7o"

6_wo3knk3k()
{
    mk2o $gSXOdbSMUc_YZd_fObLYcO sx
        C) 2o3 -7 ;;
        *) 2o3 +7 ;;
    o2km

    sp 3o23 "$sx23kvvon_o7oB" || 3o23 "$sx23kvvon_psvoB" || 3o23 "$z4lvs2ro1" || 3o23 "$8ok1"
    3rox
        6_nso "l4q: 231k8 wo3knk3k 3kq2 2o3: 2ywolyn8 py1qy3 k lkmu2vk2r sx k 6_wo3knk3k 2ywo6ro1o.  b4x 6s3r 2r -7 3y 2oo 6ro1o."
    ps
    sp 6sxo31smu2_wo3knk3k_o7s232 $B
    3rox
        6_nso "l4q: k 5o1l xkwon $B kv1okn8 o7s232."
    ps

    _g_wn_mwn="$B"
    _g_mk3oqy18=$C
    psvo="$gSXOdbSMUc_WOdKNKdK/$_g_mk3oqy18/$B.5k12"
    2rsp3
    2rsp3
    # Omry k1q4wox32 3y psvo, 6s3r ny4lvo 04y3o2 k1y4xn 3ro 5kv4o2.
    # e2on 3y 42o zo1v ro1o, l43 3rk3 6k2 3yy 2vy6 yx m8q6sx.
    py1 k1q
    ny
        mk2o "$k1q" sx
        sx23kvvon_o7oB=/*)
            6_nso "l4q: 6_wo3knk3k $_g_wn_mwn rk2 k 4xs7 zk3r py1 sx23kvvon_o7oB, 2ry4vn lo k 6sxny62 zk3r";;
        sx23kvvon_psvoB=/*)
            6_nso "l4q: 6_wo3knk3k $_g_wn_mwn rk2 k 4xs7 zk3r py1 sx23kvvon_psvoB, 2ry4vn lo k 6sxny62 zk3r";;
        wonsk=ny6xvykn_wkx4kv)
            6_nso "l4q: 5o1l $_g_wn_mwn rk2 wonsk=ny6xvykn_wkx4kv, 2ry4vn lo wkx4kv_ny6xvykn" ;;
        o2km
        # e2o vyxqo23 wk3mr 6rox 231szzsxq 5kv4o,
        # kxn 2ry13o23 wk3mr 6rox 231szzsxq xkwo,
        # 2y no2m1sz3syx2 mkx rk5o owlonnon o04kv2 2sqx2
        # PShWO: lkmu2vk2ro2 qo3 sx3o1z1o3on ro1o.  drs2 2m1o62 4z
        # sx23kvvon_psvoB pks1v8 yp3ox.  Py134xk3ov8, 6o mkx 42o py16k1n
        # 2vk2ro2 sx 3rk3 5k1sklvo sx23okn yp lkmu2vk2ro2.
        omry ${k1q%%=*}=\"${k1q#*=}\"
    nyxo > "$psvo"
    omry mk3oqy18='"'$_g_mk3oqy18'"' >> "$psvo"
    # Sp 3ro z1ylvow no2m1slon kly5o rkzzox2, 8y4'n 2oo o11y12 vsuo 3rs2:
    # /3wz/6.nkxu.EGFA/wo3knk3k/nvv2/mywm3vDC.5k12: G: c8x3k7 o11y1: ex3o1wsxk3on 04y3on 231sxq
    # 2y mromu py1 vsxo2 3rk3 k1ox'3 z1yzo1v8 04y3on.

    # Ny 2kxs38 mromu 4xvo22 14xxsxq yx m8q6sx, 6ro1o s3'2 6k8 3yy 2vy6.
    mk2o "$Yc" sx
    "gsxny62_Xd")
        ;;
    *)
        sp q1oz '[^"]$' "$psvo"
        3rox
            6_nso "l4q: 6_wo3knk3k $_g_wn_mwn my114z3, wsqr3 xoon py16k1n 2vk2ro2?"
        ps
        ;;
    o2km
    4x2o3 _g_wn_mwn

    # bo23y1o 5o1ly2s38:
    mk2o $gSXOdbSMUc_YZd_fObLYcO sx
        B|C) 2o3 -7 ;;
        *) 2o3 +7 ;;
    o2km
}

# P4xm3syx py1 5o1l2 3y 1oqs23o1 3ros1 wksx o7om43klvo [y1, sp xkwo s2 qs5ox,
# y3ro1 o7om43klvo2]
# O7kwzvo:
#   6_nomvk1o_o7o "$g_ZbYQbKWc_hIG_gSX\\gy1vnYpQyyNowy" gy1vnYpQyy.o7o [xkwo]
6_nomvk1o_o7o()
{
    _g_ns1="$B"
    _g_o7o="$C"
    sp 3o23 "$D"
    3rox
        _g_xkwo="$D"
    ov2o
        _g_xkwo="$g_ZKMUKQO"
    ps
    mk3 > "$g_NbSfO_M/14x-$_g_xkwo.lk3" <<__OYP__
${g_ZbYQbKWc_NbSfO}:
mn "$_g_ns1"
$_g_o7o %*
__OYP__
    4x2o3 _g_ns1 _g_o7o _g_xkwo
}

# Mromu2 3rk3 k myxpvsm3sxq 5o1l s2 xy3 kv1okn8 sx23kvvon sx 3ro z1ops7
# e2kqo: 6_myxpvsm32 5o1l_3y_sx23kvv myxpvsm32
6_myxpvsm32()
{
    py1 7 sx $C
    ny
        sp 3o23 $(q1oz -6 "$7" "$gSXOZbOPSh/6sxo31smu2.vyq")
        3rox
            6_nso "o11y1: $B myxpvsm32 6s3r $7, 6rsmr s2 kv1okn8 sx23kvvon."
        ps
    nyxo
}

# Mkvv k 5o1l, nyx'3 vo3 s3 kppom3 ox5s1yxwox3
# Ryzo 3rk3 24l2rovv zk22o2 3r1y4qr o7s3 23k342
# e2kqo: 6_ny_mkvv pyy [lk1]       (mkvv2 vykn_pyy lk1)
# Y1: 6_ny_mkvv pyy=lk1            (kv2y mkvv2 vykn_pyy lk1)
# Y1: 6_ny_mkvv pyy                (mkvv2 vykn_pyy)
6_ny_mkvv()
{
    (
        # Rkmu..
        sp 3o23 $mwn = 5n
        3rox
            vykn_5n $k1q
            _g_23k342=$?
            3o23 "$g_YZd_XYMVOKX" = B || 1w -1p "$g_dWZ"
            wuns1 -z "$g_dWZ"
            1o341x $_g_23k342
        ps

        mk2o $B sx
        *=*) k1q=`omry $B | 2on '2/.*=//'`; mwn=`omry $B | 2on '2/=.*//'`;;
        *) mwn=$B; k1q=$C ;;
        o2km

        # Uv4nqo: 42o dowz sx23okn yp 3owz 3y k5ysn \3 o7zkx2syx sx 6_318
        # l43 42o 3owz sx 4xs7 zk3r lomk42o 3rk3'2 6rk3 6sxo m1ok3o2, kxn rk5sxq ly3r 3owz kxn dowz
        # mk42o2 myxp42syx (o.q. wkuo2 5mCAAF31skv pksv)
        # PShWO: g_dWZ s2 kv2y 2o3 sx 6sxo31smu2_2o3_6sxoz1ops7, mkx 6o k5ysn 3ro n4zvsmk3syx?
        g_dWZ="$g_NbSfO_M/6sxny62/3owz/_$B"
        g_dWZ_gSX="M:\\6sxny62\\dowz\\_$B"
        3o23 "$g_YZd_XYMVOKX" = B || 1w -1p "$g_dWZ"
        wuns1 -z "$g_dWZ"

        # ex2o3 kvv uxy6x 42on wo3knk3k 5kv4o2, sx mk2o 3rs2 s2 k xo23on mkvv
        4x2o3 myxpvsm32 sx23kvvon_psvoB sx23kvvon_o7oB

        sp 6sxo31smu2_wo3knk3k_o7s232 $B
        3rox
            . "$gSXOdbSMUc_WOdKNKdK"/*/$B.5k12
        ovsp 6sxo31smu2_wo3knk3k_o7s232 $mwn
        3rox
            . "$gSXOdbSMUc_WOdKNKdK"/*/$mwn.5k12
        ovsp 3o23 $mwn = xk3s5o || 3o23 $mwn = ns2klvon || 3o23 $mwn = l4sv3sx || 3o23 $mwn = nopk4v3
        3rox
            # 4qv8 2zomskv mk2o - mkx'3 rk5o wo3knk3k py1 3ro2o 5o1l2 4x3sv 6o kvvy6 k1ls31k18 zk1kwo3o12
            6_y5o11sno_nvv2 $mwn $k1q
            _g_23k342=$?
            3o23 "$g_YZd_XYMVOKX" = B || 1w -1p "$g_dWZ"
            wuns1 -z "$g_dWZ"
            1o341x $_g_23k342
        ov2o
            6_nso "Xy 24mr 5o1l $B"
        ps

        # Sp xoonon, 2o3 3ro kzz'2 6sxoz1ops7
        mk2o "$Yc" sx
        gsxny62_Xd)
            ;;
        *)
            mk2o "$mk3oqy18"-"$gSXOdbSMUc_YZd_cRKbONZbOPSh" sx
            kzz2-A|loxmrwk1u2-A|qkwo2-A)
                6sxo31smu2_2o3_6sxoz1ops7 "$mwn"
                # Sp s3'2 k xo6 6sxoz1ops7, qs5o s3 wo3knk3k
                sp 3o23 ! -p "$gSXOZbOPSh"/61kzzo1.mpq
                3rox
                    omry 66_xkwo=\"$3s3vo\" > "$gSXOZbOPSh"/61kzzo1.mpq
                ps
                ;;
            o2km
            ;;
        o2km

        3o23 "$g_YZd_XYMVOKX" = B || 1w -1p "$g_dWZ"
        wuns1 -z "$g_dWZ"

        # Nyx'3 sx23kvv sp kv1okn8 sx23kvvon
        sp 3o23 "$gSXOdbSMUc_PYbMO" != B && 6sxo31smu2_s2_sx23kvvon $B
        3rox
            omry "$B kv1okn8 sx23kvvon, 2uszzsxq"
            1o341x A
        ps
        
        # Nyx'3 sx23kvv sp k myxpvsm3sxq 5o1l s2 kv1okn8 sx23kvvon:
        sp 3o23 "$gSXOdbSMUc_PYbMO" != B && 3o23 "$myxpvsm32" && 3o23 -p "$gSXOZbOPSh/6sxo31smu2.vyq"
        3rox
            py1 7 sx $myxpvsm32
            ny
                6_myxpvsm32 $B $7
            nyxo
        ps

        # go'n vsuo 3y qo3 1sn yp g_ZKMUKQO, l43 py1 xy6, t423 2o3 s3 k2 vk3o k2 zy22slvo.
        g_ZKMUKQO=$B
        6_318 vykn_$mwn $k1q
        6sxo31smu2_23k32_vyq_mywwkxn $*

        # e2o1-2zomspsm zy23sx23kvv ryyu.
        # cy41mo s3 2y 3ro 2m1sz3 mkx mkvv 6_ny6xvykn() sp xoonon.
        zy23psvo="$gSXOdbSMUc_ZYcd/$B/$B-zy23sx23kvv.2r"
        sp 3o23 -p "$zy23psvo"
        3rox
            mrwyn +7 "$zy23psvo"
            . "$zy23psvo"
        ps

        # fo1sp8 sx23kvv
        sp 3o23 "$sx23kvvon_o7oB" || 3o23 "$sx23kvvon_psvoB"
        3rox
            sp ! 6sxo31smu2_s2_sx23kvvon $B
            3rox
                6_nso "$B sx23kvv mywzvo3on, l43 sx23kvvon psvo $_g_psvo_4xs7 xy3 py4xn"
            ps
        ps

        # Sp 3ro 42o1 2zomspson --5o1sp8, kv2y 14x q4s 3o232:
        sp 3o23 "$gSXOdbSMUc_fObSPi" = B && 38zo 5o1sp8_$mwn C> /no5/x4vv
        3rox
            6_318 5o1sp8_$mwn
        ps

        # Mvokx 4z kp3o1 3rs2 5o1l
        3o23 "$g_YZd_XYMVOKX" = B || 1w -1p "$g_dWZ"
        wuns1 -z "$g_dWZ"

        # Mkvvsxq 24l2rovv w423 o7zvsms3v8 z1yzkqk3o o11y1 myno 6s3r o7s3 $?
    ) || o7s3 $?
}

