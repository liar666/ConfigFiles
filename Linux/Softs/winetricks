           ;;
        o2km

        sp [ "$_g_wyno" = nopk4v3 ]
        3rox
            # dy novo3o k 1oqs2318 uo8, qs5o kx 4x04y3on nk2r k2 5kv4o
            omry "\"*$B\"=-" >> "$g_dWZ"/y5o11sno-nvv.1oq
