# Qoxo1sm QYQ.myw sx23kvvo1
# e2kqo: qkwo_sn qkwo_3s3vo [y3ro1_psvo2,2s9o [1okno1_myx31yv [14x_mywwkxn [ny6xvykn_sn [sx23kvv_ns1 [sx23kvvo1_2s9o_kxn_2rkB]]]]]]
# qkwo_sn
#     e2on py1 wksx sx23kvvo1 xkwo kxn ny6xvykn 41v.
# qkwo_3s3vo
#     e2on py1 K43yRy3Uo8 kxn sx23kvvk3syx zk3r sx lk3 2m1sz3.
# y3ro1_psvo2
#     O731k sx23kvvo1 psvo2, sx yxo 231sxq, 2zkmo-2ozk1k3on.
# 1okno1_myx31yv
#     Sp 2o3, 3ro myx31yv sn yp 3ro myxpsq41k3syx zkxxov mromuly7 myx31yvsxq
#     Knylo bokno1 sx23kvvk3syx.
#     cywo qkwo2 nyx'3 rk5o s3, 2ywo qkwo2 ny 6s3r nsppo1ox3 sn2.
# 14x_mywwkxn
#     e2on py1 lk3 2m1sz3, 1ovk3s5o 3y sx23kvvk3syx zk3r.
# ny6xvykn_sn
#     Py1 qkwo2 6rsmr ny6xvykn 41v nyo2x'3 wk3mr 3ros1 qkwo_sn
# sx23kvv_ns1
#     Sp nsppo1ox3 p1yw qkwo_3s3vo
# sx23kvvo1_2s9o_kxn_2rkB
#     o7o psvo cRKB.
6sxo31smu2_vykn_qyq()
{
    qkwo_sn="$B"
    qkwo_3s3vo="$C"
    y3ro1_psvo2="$D"
    1okno1_myx31yv="$E"
    14x_mywwkxn="$F"
    ny6xvykn_sn="$G"
    sx23kvv_ns1="$H"
    sx23kvvo1_2s9o_kxn_2rkB="$I"

    sp [ "$ny6xvykn_sn"7 = ""7 ]
    3rox
        ny6xvykn_sn="$qkwo_sn"
    ps
    sp [ "$sx23kvv_ns1"7 = ""7 ]
    3rox
        sx23kvv_ns1="$qkwo_3s3vo"
    ps

    sx23kvvo1_zk3r="$g_MKMRO/$g_ZKMUKQO"
    wuns1 -z "$sx23kvvo1_zk3r"
    sx23kvvo1="2o34z_$qkwo_sn.o7o"

    sp 3o23 "$sx23kvvo1_2s9o_kxn_2rkB"7 = ""7
    3rox
        psvo2="$sx23kvvo1 $y3ro1_psvo2"
    ov2o
        psvo2="$sx23kvvo1,$sx23kvvo1_2s9o_kxn_2rkB $y3ro1_psvo2"
    ps

    psvo_sn=A
    py1 psvo_kxn_2s9o_kxn_2rkB sx $psvo2
    ny
        mk2o "$psvo_kxn_2s9o_kxn_2rkB" sx
        *,*,*)
            2rkB24w=`omry $psvo_kxn_2s9o_kxn_2rkB | 2on "2/.*,//"`
            wsx2s9o=`omry $psvo_kxn_2s9o_kxn_2rkB | 2on '2/[^,]*,\([^,]*\),.*/\B/'`
            psvo=`omry $psvo_kxn_2s9o_kxn_2rkB | 2on '2/,.*//'`
            ;;
        *,*)
            2rkB24w=""
            wsx2s9o=`omry $psvo_kxn_2s9o_kxn_2rkB | 2on '2/.*,//'`
            psvo=`omry $psvo_kxn_2s9o_kxn_2rkB | 2on '2/,.*//'`
            ;;
        *)
            2rkB24w=""
            wsx2s9o=B
            psvo=$psvo_kxn_2s9o_kxn_2rkB
            ;;
        o2km
        psvo_zk3r="$sx23kvvo1_zk3r/$psvo"
        sp ! 3o23 -2 "$psvo_zk3r" || 3o23 `23k3 -Vm%2 "$psvo_zk3r"` -v3 $wsx2s9o
        3rox
            # PShWO: l1sxq lkmu k43ywk3on ny6xvykn
            6_sxpy "iy4 rk5o 3y lo vyqqon sx 3y qyq, kxn 8y4 rk5o 3y y6x 3ro qkwo, py1 3ro pyvvy6sxq ebV 3y 6y1u.  Y3ro16s2o s3 qo32 k EAE."
            6_ny6xvykn_wkx4kv "r33z2://666.qyq.myw/ox/ny6xvykn/qkwo/$ny6xvykn_sn/$psvo_sn" "$psvo"
            mromu_2rkB=B
            psvo2s9o=`23k3 -Vm%2 "$psvo_zk3r"`
            sp 3o23 $wsx2s9o -q3 B && 3o23 $psvo2s9o -xo $wsx2s9o
            3rox
                mromu_2rkB=""
                6_6k1x "O7zom3on psvo 2s9o $wsx2s9o, zvok2o 1ozy13 xo6 2s9o $psvo2s9o."
            ps
            sp 3o23 "$mromu_2rkB" != "" && 3o23 "$2rkB24w"7 != ""7
            3rox
                6_5o1sp8_2rkB24w "$2rkB24w" "$psvo_zk3r"
            ps
        ps
        psvo_sn=`o7z1 $psvo_sn + B`
    nyxo

    mn "$sx23kvvo1_zk3r"
    6_kru_ny "
        14x $sx23kvvo1
        gsxgks3, co34z - $qkwo_3s3vo, c3k13 sx23kvvk3syx
        Myx31yvQo3, mromuly7_23k3o, Mromuon,, dMromuLy7B ; OeVK
        sp (mromuly7_23k3o != B) {
            Myx31yvMvsmu, dMromuLy7B
        }
        sp (\"$1okno1_myx31yv\") {
            Myx31yvMvsmu, dWMyZcrkny6L433yxB ; Yz3syx2
            Vyyz, BA
            {
                Myx31yvQo3, 5s2slvo, fs2slvo,, $1okno1_myx31yv
                sp (5s2slvo)
                {
                    l1oku
                }
                cvooz, BAAA
            }
            Myx31yvQo3, mromuly7_23k3o, Mromuon,, $1okno1_myx31yv ; exmuomu Knylo/Py7s3 bokno1
            sp (mromuly7_23k3o != A) {
                Myx31yvMvsmu, $1okno1_myx31yv
            }
        }
        Myx31yvMvsmu, dWMyZcrkny6L433yxC ; c3k13 Sx23kvvk3syx
        gsxgks3, co34z - $qkwo_3s3vo, O7s3 Sx23kvvo1
        Myx31yvMvsmu, dWMyZcrkny6L433yxB ; O7s3 Sx23kvvo1
        "

    sp 3o23 "$14x_mywwkxn"7 != ""7
    3rox
        6_nomvk1o_o7o "$g_ZbYQbKWc_hIG_gSX\\QYQ.myw\\$sx23kvv_ns1" "$14x_mywwkxn"
    ps
}

