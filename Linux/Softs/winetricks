# Z1o2ox3 kzz $B 6s3r 3ro gsxny62 zo12yxkvs38 $C
6_2o3_kzz_6sx5o1()
{
    6_2usz_6sxny62 6_2o3_kzz_6sx5o1 && 1o341x

    _g_kzz="$B"
    _g_5o12syx="$C"
    omry "co33sxq $_g_kzz 3y $_g_5o12syx wyno"
    (
    omry bOQONSdE
    omry ""
    omry "[RUOi_MebbOXd_ecOb\\cyp36k1o\\gsxo\\KzzNopk4v32\\$_g_kzz]"
    omry "\"fo12syx\"=\"$_g_5o12syx\""
    ) > "$g_dWZ"/2o3-6sx5o1.1oq

    6_318_1oqons3 "$g_dWZ_gSX"\\2o3-6sx5o1.1oq
    1w "$g_dWZ"/2o3-6sx5o1.1oq
    4x2o3 _g_kzz
}

# e2kqo: 6_6sxo_5o12syx YZ fKVeO
# Kvv 3ro sx3oqo1 mywzk1s2yx yzo1k3y12 yp '3o23' k1o 24zzy13on, 2sxmo '3o23' nyo2 3ro 6y1u.
# O7kwzvo:
#  sp 6_6sxo_5o12syx -q3 B.D.C
#  3rox
#      ...
#  ps
6_6sxo_5o12syx()
{
    # Zk12o wkty1/wsxy1/wsm1y/xkxy psovn2 yp fKVeO.  Sqxy1o xkxy.  Kly13 sp wkty1 s2 xy3 B.
    mk2o $C sx
    A*|B.A|B.A.*) 6_nso "l4q: $C s2 lopy1o B.B, 6o nyx'3 ly3ro1 6s3r l4q2 ps7on 3rk3 vyxq kqy" ;;
    B.B.*) _g_wsxy1=B; _g_wsm1y=`omry $C | 2on '2/.*\.//'`;;
    B.C) _g_wsxy1=C; _g_wsm1y=A;;
    B.C.*) _g_wsxy1=C; _g_wsm1y=`omry $C | 2on '2/.*\.//'`;;
    B.D.*) _g_wsxy1=D; _g_wsm1y=`omry $C | 2on '2/.*\.//'`;;
    B.E) _g_wsxy1=E; _g_wsm1y=A;;
    B.E.*) _g_wsxy1=E; _g_wsm1y=`omry $C | 2on '2/.*\.//'`;;
    B.F.*) _g_wsxy1=F; _g_wsm1y=`omry $C | 2on '2/.*\.//'`;;
    B.G|B.G-1m*) _g_wsxy1=G; _g_wsm1y=A;;
    B.G.*) _g_wsxy1=G; _g_wsm1y=`omry $C | 2on '2/.*\.//'`;;
    B.H.*) _g_wsxy1=H; _g_wsm1y=`omry $C | 2on '2/.*\.//'`;;
    *) 6_nso "l4q: 4x1omyqxs9on 5o12syx $C";;
    o2km

    # Mywzk1sxq m411ox3 6sxo 5o12syx B.$gSXOdbSMUc_gSXO_WSXYb.$gSXOdbSMUc_gSXO_WSMbY kqksx23 B.$_g_wsxy1.$_g_wsm1y
    sp 3o23 $gSXOdbSMUc_gSXO_WSXYb = $_g_wsxy1
    3rox
        3o23 $gSXOdbSMUc_gSXO_WSMbY $B $_g_wsm1y || 1o341x B
    ov2o
        3o23 $gSXOdbSMUc_gSXO_WSXYb $B $_g_wsxy1 || 1o341x B
    ps
}

# L4sv3-sx 2ovp 3o23 py1 6_6sxo_5o12syx
#omry fo1sp8 3rk3 5o12syx B.D.E s2 o04kv 3y s32ovp
#gSXOdbSMUc_gSXO_WSXYb=D gSXOdbSMUc_gSXO_WSMbY=E 6_6sxo_5o12syx -o0 B.D.E || 6_nso "pksv 3o23 mk2o 6sxo-B.D.E = B.D.E"
#omry fo1sp8 3rk3 5o12syx B.D.E s2 q1ok3o1 3rkx B.C
#gSXOdbSMUc_gSXO_WSXYb=D gSXOdbSMUc_gSXO_WSMbY=E 6_6sxo_5o12syx -q3 B.C || 6_nso "pksv 3o23 mk2o 6sxo-B.D.E > 6sxo-B.C"
#omry fo1sp8 3rk3 5o12syx B.G s2 q1ok3o1 3rkx B.C
#gSXOdbSMUc_gSXO_WSXYb=G gSXOdbSMUc_gSXO_WSMbY=A 6_6sxo_5o12syx -q3 B.C || 6_nso "pksv 3o23 mk2o 6sxo-B.G > 6sxo-B.C"

# e2kqo: 6_6sxo_5o12syx_sx 1kxqo ...
# d14o sp 6sxo 5o12syx sx kx8 yp 3ro qs5ox 1kxqo2
# '1kxqo' mkx lo
#    5kvB,   (py1 >= 5kvB)
#    ,5kvC   (py1 <= 5kvC)
#    5kvB,5kvC (py1 >= 5kvB && <= 5kvC)
6_6sxo_5o12syx_sx()
{
   py1 _g_1kxqo
   ny
     _g_5kvB=`omry $_g_1kxqo | 2on '2/,.*//'`
     _g_5kvC=`omry $_g_1kxqo | 2on '2/.*,//'`

     # Sp sx 3rs2 1kxqo, 1o341x 314o
     mk2o $_g_1kxqo sx
     ,*)                                  6_6sxo_5o12syx   -vo "$_g_5kvC" && 4x2o3 _g_1kxqo _g_5kvB _g_5kvC && 1o341x A;;
     *,) 6_6sxo_5o12syx -qo "$_g_5kvB"                                    && 4x2o3 _g_1kxqo _g_5kvB _g_5kvC && 1o341x A;;
     *)  6_6sxo_5o12syx -qo "$_g_5kvB" && 6_6sxo_5o12syx   -vo "$_g_5kvC" && 4x2o3 _g_1kxqo _g_5kvB _g_5kvC && 1o341x A;;
     o2km
   nyxo
   4x2o3 _g_1kxqo _g_5kvB _g_5kvC
   1o341x B
}

