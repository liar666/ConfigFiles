# Myx5o132 k wo3knk3k klyv43o zk3r 3y s32 kzz myno
6sxo31smu2_wo3knk3k_lk2oxkwo()
{
    # Mvk22sm, l43 3yy 2vy6 yx m8q6sx
    #lk2oxkwo $B .5k12

    # ps123, 1owy5o 24pps7 .5k12
    _g_wl_3wz=${B%.5k12}
    # 2omyxn, 1owy5o kx8 ns1om3y18 z1ops7
    omry ${_g_wl_3wz##*/}
    4x2o3 _g_wl_3wz
}

# bo341x2 314o sp qs5ox 5o1l rk2 loox 1oqs23o1on
