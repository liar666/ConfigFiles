#----------------------------------------------------------------
# co33sxq2
#----------------------------------------------------------------
# Ns1om3DN 2o33sxq2

6sxo31smu2_2o3_6sxonDn_5k1()
{
    # Psv3o1 y43/my11om3 lkn y1 zk13skv 5kv4o2
    # Myxp42sxq lomk42o nsxz43 42o2 'ns2klvo', l43 nDn 42o2 'ns2klvon'
    # 2oo 6sxonDn_nvv_sxs3() sx nvv2/6sxonDn/6sxonDn_wksx.m
    # kxn NvvWksx() sx nvv2/nn1k6/wksx.m
    mk2o $C sx
    ns2klvo*) k1q=ns2klvon;;
    oxklvo*) k1q=oxklvon;;
    rk1n*) k1q=rk1n6k1o;;
    1ozkmu) k1q=1ozkmu;;
    lkmul4ppo1|ply|qns|xyxo|yzoxqv|1oknn1k6|1okn3o7|3o7n1k6|3o73o7|k43y) k1q=$C;;
    [A-J]*) k1q=$C;;
    *) 6_nso "svvoqkv 5kv4o $C py1 $B";;
    o2km

    omry "co33sxq Ns1om3DN/$B 3y $k1q"
    mk3 > "$g_dWZ"/2o3-6sxonDn.1oq <<_OYP_
bOQONSdE

[RUOi_MebbOXd_ecOb\cyp36k1o\gsxo\Ns1om3DN]
"$B"="$k1q"

_OYP_
    6_318_1oqons3 "$g_dWZ_gSX"\\2o3-6sxonDn.1oq
}

