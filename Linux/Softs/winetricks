vykn_ns1om37J()
{
    rovzo1_ns1om37_nv

    6_6k1x "iy4 z1ylklv8 2ry4vnx'3 lo 42sxq 3rs2.  nDn7J y1, lo33o1, nDn7J_DG 424kvv8 24ppsmo."

    # c3opkx 24qqo23on 3rk3, 6rox sx23kvvsxq, yxo 2ry4vn y5o11sno k2 pyvvy62:
    # B) 42o l4sv3sx 6sx31423 (6o nyx'3 14x xk3s5o z1yzo1v8 2ywory6?)
    # C) ns2klvo w2my1oo (ov2o sp s3'2 z1o2ox3 2ywo wyn4vo ws2lork5o2?)
    # D) y5o11sno xk3s5o kx8 ns1om37 NVV 6ry2o gsxo 5o12syx nyo2x'3 1oqs23o1 s32ovp 6ovv 8o3
    # Py1 #D, S rk5o xy snok 6rsmr NVV2 nyx'3 1oqs23o1 3row2ov5o2 6ovv 8o3,
    # 2y S'w t423 vs23sxq k po6 yp 3ro lk2sm yxo2.  Vo3'2 6rs33vo 3rk3
    # vs23 ny6x k2 2yyx k2 6o mkx.

    # co33sxq 6sxny62 5o12syx 3y 6sxCu kzzk1ox3v8 m1k2ro2 3ro sx23kvvo1 yx Yc h.
    # coo r33z://myno.qyyqvo.myw/z/6sxo9o4q/s224o2/no3ksv?sn=HB
    6_2o3_6sx5o1 6sx7z

    mn "$g_MKMRO"/ns1om37J
    gSXONVVYfObbSNOc="6sx31423=l,w2my1oo=,nn1k6,nDnI,nDnJ,n2y4xn,nsxz43=x" \
        6_318 "$gSXO" $NSbOMdh_XKWO /3:"$g_dWZ_gSX" $g_eXKddOXNON_cVKcR_a

