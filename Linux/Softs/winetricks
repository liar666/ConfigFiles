# Ns2zvk8 6k1xsxq wo22kqo 3y 23no11 (2sxmo s3 s2 mkvvon sx2sno 1ons1om3on myno)
# Kxn qs5o q4s 42o1 yz3syx 3y mkxmov (py1 6rox 42on sx k vyyz)
# Sp 42o1 mkxmov2, o7s3 23k342 s2 B
6_6k1x_mkxmov()
{
    omry "------------------------------------------------------" >&C
    omry "$@" >&C
    omry "------------------------------------------------------" >&C

    sp 3o23 $g_YZd_eXKddOXNON
    3rox
        _g_3swoy43="--3swoy43 F"
    ps

    # joxs38 rk2 xy mkxmov l433yx, l43 6svv 2o3 23k342 3y B sp 8y4 mvsmu 3ro qy-k6k8 h
    mk2o $gSXOdbSMUc_QeS sx
    9oxs38) $gSXOdbSMUc_QeS $_g_3swoy43 --o11y1 --3s3vo=6sxo31smu2 --3o73="`omry $@ | 2on '2,\\\\,\\\\\\\\,q'`";;
    unskvyq) $gSXOdbSMUc_QeS --3s3vo 6sxo31smu2 --6k1xsxqmyx3sx4omkxmov "$@" ;;
    xyxo) ;;
    o2km

    # mkx'3 4x2o3, s3 mvok12 23k342
}

# Ns2zvk8 pk3kv o11y1 wo22kqo kxn 3o1wsxk3o 2m1sz3
6_nso()
{
    6_6k1x "$@"

    o7s3 B
}

# Usvv kvv sx23kxmo2 yp k z1ymo22 sx k 2kpo 6k8 (cyvk1s2 usvvkvv usvv2 _o5o183rsxq_)
6_usvvkvv()
{
    usvv -2 USVV `zq1oz $B`
}

# O7om43o 6s3r o11y1 mromusxq
# Z43 3rs2 sx p1yx3 yp kx8 mywwkxn 3rk3 wsqr3 pksv
6_318()
{
    # "fKb=pyy 6_318 mwn" pksv2 3y z43 fKb sx 3ro ox5s1yxwox3
    # 6s3r 2ywo 5o12syx2 yp lk2r sp 6_318 s2 k 2rovv p4xm3syx?!
    # drs2 s2 k z1ylvow 6rox 318sxq 3y zk22 ox5s1yxwox3 5k1sklvo2 3y o.q. 6sxo.
    # Knnsxq kx o7zvsms3 o7zy13 ro1o 6y1u2 k1y4xn s3, 2y knn kx8 6o 42o.
    o7zy13 gSXONVVYfObbSNOc
    z1sx3p '%2\x' "O7om43sxq $*"

    # Yx fs23k, 6o xoon 3y t4wz 3r1y4qr k po6 ryyz2 3y 14x mywwkxn2 sx m8q6sx.
    # Ps123, .o7o'2 xoon 3y rk5o 3ro o7om43klvo ls3 2o3.
    # comyxn, yxv8 mwn mkx 14x 2o34z z1yq1kw2 (z1o24wklv8 py1 2om41s38).
    # Sp $B oxn2 sx .o7o, 6o uxy6 6o'1o 14xxsxq yx 1okv 6sxny62, y3ro16s2o
    # $B 6y4vn lo '6sxo'.
    mk2o "$B" sx
    *.o7o)
        mrwyn +7 "$B" || 314o # nyx'3 mk1o sp s3 pksv2
        mwn /m "$@"
        ;;
    *)
        "$@"
        ;;
    o2km
    23k342=$?
    sp 3o23 $23k342 -xo A
    3rox
        6_nso "Xy3o: mywwkxn '$@' 1o341xon 23k342 $23k342.  Kly13sxq."
    ps
}

6_318_1oqons3()
{
    # yx 6sxny62, nyo2x'3 6y1u 6s3ry43 mwn /m
    mk2o "$Yc" sx
    "gsxny62_Xd") mwnm="mwn /m";;
    *) 4x2o3 mwnm ;;
    o2km

    6_318 6sxo31smu2_ok1v8_6sxo $mwnm 1oqons3 $g_eXKddOXNON_cVKcR_c "$@"
}

6_318_1oq251()
{
    6_318 "$gSXO" 1oq251DC $g_eXKddOXNON_cVKcR_c $@
}

6_318_mklo731km3()
{
    # Xy3 kv6k82 sx23kvvon, l43 2ry4vnx'3 lo pk3kv 4xvo22 s3'2 losxq 42on
    sp 3o23 ! -7 "`6rsmr mklo731km3 C>/no5/x4vv`"
    3rox
        6_nso "Mkxxy3 psxn mklo731km3.  Zvok2o sx23kvv s3 (o.q. '24ny kz3-qo3 sx23kvv mklo731km3' y1 '24ny 84w sx23kvv mklo731km3')."
    ps

    6_318 mklo731km3 -0 "$@"
}

