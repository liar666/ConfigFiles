        (
            6qo3 "$@" C>&B |
            6sxo31smu2_zk12o_6qo3_z1yq1o22 | \
            $gSXOdbSMUc_QeS --z1yq1o22 --6sn3r EAA --3s3vo="$_g_psvo" --k43y-usvv --k43y-mvy2o
        )
        o11=$?
        sp 3o23 $o11 -q3 BCI
        3rox
            # BCJ s2 'usvvon l8 cSQReZ'
            # cknv8, --k43y-usvv yxv8 kzzvso2 3y zk1ox3 z1ymo22,
            # 6rsmr 6k2 3ro 24l2rovv, xy3 kvv 3ro ovowox32 yp 3ro zszovsxo...
            # rk5o 3y qy psxn kxn usvv 3ro 6qo3.
            # Sp 6o 1kx 6qo3 sx 3ro lkmuq1y4xn, 6o my4vn usvv s3 wy1o ns1om3v8, zo1rkz2...
