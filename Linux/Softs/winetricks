    *7wo22kqo) 6_nso "2y118, mkx'3 k2u 04o23syx 6s3r 7wo22kqo" ;;
    xyxo) omry -x "$B" >&C ; 1okn g_KXcgOb ; omry $g_KXcgOb; 4x2o3 g_KXcgOb;;
    o2km
}

# bokn2 23okw 42o1xkwo kxn zk226y1n p1yw ox5s1yxwox3, mkmro, y1 42o1
# Sp rkn 3y k2u 42o1, mkmro kx26o1.
6_23okw_qo3sn()
{
    #dYNY: d1kx2vk3o
    _g_23okwsnw2q="Zvok2o ox3o1 8y41 c3okw vyqsx SN (xy3 owksv)"
    _g_23okwzk226y1nw2q="Zvok2o ox3o1 8y41 c3okw zk226y1n"

    sp 3o23 ! "$g_cdOKW_SN"
    3rox
        sp 3o23 -p "$g_MKMRO"/23okw_42o1sn.373
        3rox
            g_cdOKW_SN=`mk3 "$g_MKMRO"/23okw_42o1sn.373`
        ov2o
            g_cdOKW_SN=`6_04o23syx "$_g_23okwsnw2q"`
            omry "$g_cdOKW_SN" > "$g_MKMRO"/23okw_42o1sn.373
            mrwyn GAA "$g_MKMRO"/23okw_42o1sn.373
        ps
    ps
    sp 3o23 ! "$g_cdOKW_ZKccgYbN"
    3rox
        sp 3o23 -p "$g_MKMRO"/23okw_zk226y1n.373
        3rox
            g_cdOKW_ZKccgYbN=`mk3 "$g_MKMRO"/23okw_zk226y1n.373`
        ov2o
            g_cdOKW_ZKccgYbN=`6_04o23syx "$_g_23okwzk226y1nw2q"`
            omry "$g_cdOKW_ZKccgYbN" > "$g_MKMRO"/23okw_zk226y1n.373
            mrwyn GAA "$g_MKMRO"/23okw_zk226y1n.373
        ps
    ps
}

