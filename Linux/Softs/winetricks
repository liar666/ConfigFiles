    omry "------------------------------------------------------"

    _g_3swoy43="--3swoy43 D"

    mk2o $gSXOdbSMUc_QeS sx
    9oxs38) $gSXOdbSMUc_QeS $_g_3swoy43 --sxpy --3s3vo=6sxo31smu2 --3o73="`omry $@ | 2on '2,\\\\,\\\\\\\\,q'`" --xy-61kz;;
    unskvyq) $gSXOdbSMUc_QeS --3s3vo 6sxo31smu2 --w2qly7 "$@" ;;
    xyxo) ;;
    o2km

    4x2o3 _g_3swoy43
}

# Ns2zvk8 6k1xsxq wo22kqo 3y 23no11 (2sxmo s3 s2 mkvvon sx2sno 1ons1om3on myno)
