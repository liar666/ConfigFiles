vykn_Dnwk1uCAAA()
{
    # r33z://666.p4341owk1u.myw/ny6xvykn/Dnwk1uCAAA/
    sp ! 3o23 -p "$g_MKMRO/$g_ZKMUKQO/Dnwk1uCAAA_5BB_BAADAI.o7o"
    3rox
        6_ny6xvykn r33z://666.ymsx2sno.no/ny6xvykn/Dnwk1uCAAA_5BB_BAADAI.o7o lAEAAnFJmpnEFnImIIJDnDnEonmFIlGCIFooBFAC
    ps

    mn "$g_dWZ"
    wuns1 $g_ZKMUKQO
    mn $g_ZKMUKQO
    6_318_4x9sz "$g_MKMRO/$g_ZKMUKQO"/Dnwk1uCAAA_5BB_BAADAI.o7o
    6_kru_ny "
        co3ds3voWk3mrWyno, C
        14x co34z.o7o
        gsxgks3 govmywo
        ;Myx31yvMvsmu L433yxB  ; Xo73
        cvooz BAAA
        coxn {Ox3o1}           ; Xo73
        gsxgks3 Vsmox2o
        ;Myx31yvMvsmu L433yxC  ; io2
        cvooz BAAA
        coxn {Ox3o1}           ; io2
        ;gsxgks3Mvy2o kru_mvk22 #DCHHA ; Vsmox2o
        gsxgks3 kru_mvk22 #DCHHA, No23sxk3syx
        ;Myx31yvMvsmu L433yxB  ; Xo73
        cvooz BAAA
        coxn {Ox3o1}           ; Xo73
        ;gsxgks3Mvy2o kru_mvk22 #DCHHA ; No23sxk3syx
        gsxgks3, c3k13
        ;Myx31yvMvsmu L433yxB  ; Xo73
        cvooz BAAA
        coxn {Ox3o1}           ; Xo73
        gsxgks3 boqs231k3syx
        Myx31yvMvsmu L433yxB  ; Xo73
        gsxgks3 Mywzvo3o
        cvooz BAAA
        Myx31yvMvsmu L433yxB  ; exmvsmu fso6 boknwo
        ;Myx31yvMvsmu L433yxE  ; Psxs2r
        coxn {Ox3o1}           ; Psxs2r
