        6_6k1x "iy4 k1o 42sxq k GE-ls3 gSXOZbOPSh. Sp 8y4 oxmy4x3o1 z1ylvow2, zvok2o 1o3o23 sx k mvokx DC-ls3 gSXOZbOPSh lopy1o 1ozy13sxq k l4q."
    ov2o
        g_KbMR=6sxDC
        g_cicdOWDC_NVVc="$g_gSXNSb_eXSh/2823owDC"
        g_cicdOWDC_NVVc_gSX="M:\\6sxny62\\2823owDC"
