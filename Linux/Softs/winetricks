6sxo31smu2_2o3_4xk33oxnon()
{
    # go 2ry4vnx'3 42o kvv 3ro2o o731k 5k1sklvo2.  Sx23okn, 6o 2ry4vn
    # 42o ${pyy:+lk1} 3y tkw sx mywwkxnvsxo yz3syx2 py1 2svox3 sx23kvv
    # yxv8 sp g_YZd_eXKddOXNON s2 xyxowz38.  coo
    # r33z://z4l2.yzoxq1y4z.y1q/yxvsxoz4l2/JGJJJBJHJJ/43svs3so2/fD_mrkzAC.r3wv#3kq_BI_AG_AC
    # cy sx k33oxnon wyno, g_YZd_eXKddOXNON 2ry4vn lo owz38.

    mk2o $B sx
    B)
        g_YZd_eXKddOXNON=B
        # Wsqr3 6kx3 3y 31sw y41 23klvo yp 5k1sklvo2 ro1o k ls3...
        g_eXKddOXNON_NKcR_a="-0"
        g_eXKddOXNON_cVKcR_a="/0"
