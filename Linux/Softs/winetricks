    6_ny6xvykn r33z://ny6xvykn.yvnkzz2.myw/a4smu3swo/04smu3swozvk8o1HC.o7o llIJJIBpBAmpCBnoFHlJEFDoFDmpIBlJBJECHBkJ

    4x2o3 aeSMUdSWO_aeSOd
    sp 3o23 "$g_eXKddOXNON_cVKcR_a" != ""
    3rox
        aeSMUdSWO_aeSOd="/0x"  # SccOdeZNbSfOX=A
    ps

    sp 6_6y1uk1y4xn_6sxo_l4q JDGG ""  B.B.CC,
    3rox
        6_mkvv qnszv42
        6_mkvv 5m14xCAAF      # xy l4q x4wlo1, l43 2kwo o1k
    ps

    sp 6_6y1uk1y4xn_6sxo_l4q BDEH
    3rox
        6_6k1x "co33sxq 5s23k wyno 3y k5ysn lvkmusxq 3ro 6ryvo 2m1oox sx 04smu3swo"
        6_2o3_6sx5o1 5s23k
