            w8o7om="O7om=ox5 gSXOZbOPSh=\"$gSXOZbOPSh\" 6sxo-ry3ps7-GJHB mwn /m 'M:\\\\\\\\b4x-$g_ZKMUKQO.lk3'"

            w8wox4="$RYWO/No2u3yz/K22k22sx'2 M1oon L1y3ro1ryyn.no2u3yz"
