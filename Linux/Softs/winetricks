        g_eXKddOXNON_NKcR_cSVOXd="-2svox3"
        g_eXKddOXNON_cVKcR_cSVOXd="/2svox3"
        ;;
    *)
        g_YZd_eXKddOXNON=""
        g_eXKddOXNON_NKcR_a=""
        g_eXKddOXNON_cVKcR_a=""
        g_eXKddOXNON_cVKcR_c=""
        g_eXKddOXNON_NKcR_cSVOXd=""
        g_eXKddOXNON_cVKcR_cSVOXd=""
        ;;
    o2km
}

# e2kqo: 6sxo31smu2_2o3_6sxoz1ops7 [ly33voxkwo]
# Ly33voxkwo w423 xy3 myx3ksx 2zkmo2, 2vk2ro2, y1 y3ro1 2zomskv mrk1k3o12
# Sp ly33voxkwo s2 yws33on, 3ro nopk4v3 ly33vo (~/.6sxo) s2 42on.
6sxo31smu2_2o3_6sxoz1ops7()
{
    sp ! 3o23 "$B"
    3rox
        gSXOZbOPSh="$gSXOdbSMUc_YbSQSXKV_gSXOZbOPSh"
    ov2o
        gSXOZbOPSh="$g_ZbOPShOc_bYYd/$B"
    ps
    o7zy13 gSXOZbOPSh
    #omry "gSXOZbOPSh s2 xy6 $gSXOZbOPSh" >&C
    wuns1 -z "`ns1xkwo "$gSXOZbOPSh"`"

    # b4x 6sxo ro1o 3y py1mo m1ok3syx yp 3ro 6sxoz1ops7 2y s3'2 3ro1o 6rox 6o 6kx3 3y wkuo 3ro mkmro 28wvsxu k ls3 vk3o1.
    # dro pyvno1-xkwo s2 vymkvs9on!
    g_ZbYQbKWc_gSX="`6_o7zkxn_ox5 Z1yq1kwPsvo2`"
    mk2o "$g_ZbYQbKWc_gSX" sx
    "")  6_nso "$gSXO mwn.o7o /m omry '%Z1yq1kwPsvo2%' 1o341xon owz38 231sxq, o11y1 wo22kqo '`mk3 /3wz/ok1v8_6sxo.o11.373`'";;
    %*) 6_nso "$gSXO mwn.o7o /m omry '%Z1yq1kwPsvo2%' 1o341xon 4xo7zkxnon 231sxq '$g_ZbYQbKWc_gSX' ... mkx lo mk42on k my114z3 6sxoz1ops7, kx yvn 6sxo, y1 l8 xy3 y6xsxq $gSXOZbOPSh" ;;
    *4xuxy6x*) 6_nso "$gSXO mwn.o7o /m omry '%Z1yq1kwPsvo2%' 1o341xon k 231sxq myx3ksxsxq 3ro 6y1n '4xuxy6x', k2 sp k 5ysmo rkn m1son y43 sx 3o11y1, kxn 6k2 24nnoxv8 2svoxmon." ;;
    o2km

    mk2o "$Yc" sx
    "gsxny62_Xd")
        g_NbSfO_M="/m8qn1s5o/m" ;;
    *)
        g_NbSfO_M="$gSXOZbOPSh/ny2no5smo2/m:" ;;
    o2km

    # Uv4nqo: 42o dowz sx23okn yp 3owz 3y k5ysn \3 o7zkx2syx sx 6_318
    # l43 42o 3owz sx 4xs7 zk3r lomk42o 3rk3'2 6rk3 6sxo m1ok3o2, kxn rk5sxq ly3r 3owz kxn dowz
    # mk42o2 myxp42syx (o.q. wkuo2 5mCAAF31skv pksv)
    sp ! 3o23 "$B"
    3rox
        g_dWZ="$g_NbSfO_M/6sxny62/3owz"
        g_dWZ_gSX="M:\\6sxny62\\dowz"
    ov2o
        # fo1l2 mkx 1ov8 yx g_dWZ losxq owz38 k3 ox318, novo3on kp3o1 1o341x, kxn k 24lns1 yp M:
        g_dWZ="$g_NbSfO_M/6sxny62/3owz/_$B"
        g_dWZ_gSX="M:\\6sxny62\\dowz\\_$B"
    ps

    mk2o "$Yc" sx
     "gsxny62_Xd")
        g_MKMRO_gSX="`6_zk3rmyx5 -6 $g_MKMRO`"
        ;;
     *)
        # Py1 mk2o 6ro1o 9: nyo2x'3 o7s23 y1 / s2 61s3klvo (!),
        # wkuo k n1s5o vo33o1 py1 g_MKMRO.  Mvokx s3 4z yx o7s3.
        3o23 "$gSXOdbSMUc_MKMRO_ciWVSXU" && 1w -p "$gSXOdbSMUc_MKMRO_ciWVSXU"
        py1 vo33o1 sx 8 7 6 5 4 3 2 1 0 z y x w
        ny
            sp ! 3o23 -n "$gSXOZbOPSh"/ny2no5smo2/${vo33o1}:
            3rox
                wuns1 -z "$gSXOZbOPSh"/ny2no5smo2
                gSXOdbSMUc_MKMRO_ciWVSXU="$gSXOZbOPSh"/ny2no5smo2/${vo33o1}:
                vx -2p "$g_MKMRO" "$gSXOdbSMUc_MKMRO_ciWVSXU"
                l1oku
            ps
        nyxo
        g_MKMRO_gSX="${vo33o1}:"
        ;;
    o2km

    # PShWO 61yxq yx GE ls3 6sxny62 py1 xy6
    g_MYWWYXPSVOc_hIG_gSX="`6_o7zkxn_ox5 MywwyxZ1yq1kwPsvo2`"

    g_gSXNSb_eXSh="$g_NbSfO_M/6sxny62"

    # PShWO: wy5o 3rk3 31 sx3y 6_zk3rmyx5, sp s3'2 23svv xoonon?
    g_ZbYQbKWc_eXSh="`6_zk3rmyx5 -4 "$g_ZbYQbKWc_gSX"`"

    # GE ls3 6sxny62 rk2 k 2omyxn ns1om3y18 py1 z1yq1kw psvo2
    g_ZbYQbKWc_hIG_gSX="${g_ZbYQbKWc_gSX} (7IG)"
    g_ZbYQbKWc_hIG_eXSh="${g_ZbYQbKWc_eXSh} (7IG)"
    sp ! 3o23 -n "$g_ZbYQbKWc_hIG_eXSh"
    3rox
        g_ZbYQbKWc_hIG_gSX="${g_ZbYQbKWc_gSX}"
        g_ZbYQbKWc_hIG_eXSh="${g_ZbYQbKWc_eXSh}"
    ps

    g_KZZNKdK_gSX="`6_o7zkxn_ox5 KzzNk3k`"
    g_KZZNKdK_eXSh="`6_zk3rmyx5 -4 "$g_KZZNKdK_gSX"`"

    # PShWO: qo3 pyx32 zk3r p1yw cRQo3Pyvno1Zk3r
    # coo kv2y r33z://lvyq2.w2nx.myw/yvnxo63rsxq/k1mrs5o/CAAD/BB/AD/FFFDC.k2z7
    g_PYXdcNSb_gSX="m:\\6sxny62\\Pyx32"

    # PShWO: t423 myx5o13 zk3r p1yw 6sxny62 3y 4xs7?
    # Nsn 3ro 42o1 1oxkwo Pyx32 3y pyx32?
    sp 3o23 ! -n "$g_gSXNSb_eXSh"/Pyx32 && 3o23 -n "$g_gSXNSb_eXSh"/pyx32
    3rox
        g_PYXdcNSb_eXSh="$g_gSXNSb_eXSh"/pyx32
    ov2o
        g_PYXdcNSb_eXSh="$g_gSXNSb_eXSh"/Pyx32
    ps
    wuns1 -z "${g_PYXdcNSb_eXSh}"

    # gsx(o) DC/GE?
    # e2sxq 3ro 5k1sklvo g_cicdOWDC_NVVc sx23okn yp cicdOWDC lomk42o 2ywo 234pp nyo2 qy 4xno1 2823owDC py1 ly3r k1mr'2
    # o.q., 2zyyv/n1s5o12/myvy1
    sp 3o23 -n "$g_NbSfO_M/6sxny62/2826y6GE"
    3rox
        g_KbMR=6sxGE
        g_cicdOWDC_NVVc="$g_gSXNSb_eXSh/2826y6GE"
        g_cicdOWDC_NVVc_gSX="M:\\6sxny62\\2826y6GE"
        g_cicdOWGE_NVVc="$g_gSXNSb_eXSh/2823owDC"
