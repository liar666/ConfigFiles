# Ns2zvk8 wksx wox4, qo3 6rsmr 24lwox4 3ro 42o1 6kx32
6sxo31smu2_wksxwox4()
{
    mk2o $VKXQ sx
    nk*) _g_w2q_3s3vo='fðvq ox zkuuo-uk3oqy1s'
         _g_w2q_lyn8='R5kn Âx2uo1 n4 k3 qÂ1o?'
         _g_w2q_nvv2="Sx23kvv k gsxny62 NVV"
         _g_w2q_pyx32='Sx23kvv k pyx3'
         _g_w2q_2o33sxq2='Mrkxqo gsxo 2o33sxq2'
         _g_w2q_6sxompq='b4x 6sxompq'
         _g_w2q_1oqons3='b4x 1oqons3'
         _g_w2q_3k2uwq1='b4x 3k2uwq1'
