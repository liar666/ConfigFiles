# d41x ypp xo62, y5o1vk82, kxn p1soxn sx3o1km3syx sx 23okw
# b4x p1yw sx2sno m:\Z1yq1kw Psvo2\c3okw
6_23okw_2kpowyno()
{
    mk3 > "$g_dWZ/23okwmyxpsq.zv" <<"_OYP_"
#!/421/lsx/zo1v
# Zk12o 23okw'2 vymkvmyxpsq.5mp, knn 2o33sxq2 3y s3, kxn 61s3o s3 y43 kqksx
# dro psvo s2 k 1om412s5o nsm3syxk18
#
# PSVO :== MYXdKSXOb
#
# fKVeO :== "xkwo" "5kv4o" XOgVSXO
#
# MYXdKSXOb :== "xkwo" XOgVSXO "{" XOgVSXO ( fKVeO | MYXdKSXOb ) * "}" XOgVSXO
#
# go vykn s3 sx3y k 1om412s5o rk2r.

42o 231sm3;
42o 6k1xsxq2;

24l 1okn_sx3y_myx3ksxo1{
    w8( $zmyx3ksxo1 ) = @_;

    $_ = <PSVO> || 6_nso "Mkx'3 1okn ps123 vsxo yp myx3ksxo1";
    /{/ || 6_nso "Ps123 vsxo yp myx3ksxo1 6k2 xy3 {";
    6rsvo (<PSVO>) {
       mrywz;
       sp (/"([^"]*)"\2*"([^"]*)"$/) {
           ${$zmyx3ksxo1}{$B} = $C;
       } ov2sp (/"([^"]*)"$/) {
           w8( %xo6myx, $xkwo );
           $xkwo = $B;
           1okn_sx3y_myx3ksxo1(\%xo6myx);
           ${$zmyx3ksxo1}{$xkwo} = \%xo6myx;
        } ov2sp (/}/) {
           1o341x;
        } ov2o {
           6_nso "r4r?";
        }
    }
}

24l n4wz_myx3ksxo1{
    w8( $zmyx3ksxo1, $sxnox3 ) = @_;
    py1okmr (2y13(uo82(%{$zmyx3ksxo1}))) {
        w8( $5kv ) = ${$zmyx3ksxo1}{$_};
        sp (1op $5kv o0 'RKcR') {
            z1sx3 "${sxnox3}\"$_\"\x";
            z1sx3 "${sxnox3}{\x";
            n4wz_myx3ksxo1($5kv, "$sxnox3\3");
            z1sx3 "${sxnox3}}\x";
        } ov2o {
            z1sx3 "${sxnox3}\"${_}\"\3\3\"$5kv\"\x";
        }
    }
}

# Ns2klvo kx83rsxq 4x2kpo y1 kxxy8sxq
24l ns2klvo_xy3spsmk3syx2{
    w8( $zmyx3ksxo1 ) = @_;
    ${$zmyx3ksxo1}{"p1soxn2"}{"Zo12yxkc3k3oNo2s1on"} = "B";
    ${$zmyx3ksxo1}{"p1soxn2"}{"Xy3spsmk3syx2_cry6Sxqkwo"} = "A";
    ${$zmyx3ksxo1}{"p1soxn2"}{"cy4xn2_Zvk8Sxqkwo"} = "A";
    ${$zmyx3ksxo1}{"p1soxn2"}{"Xy3spsmk3syx2_cry6Yxvsxo"} = "A";
    ${$zmyx3ksxo1}{"p1soxn2"}{"cy4xn2_Zvk8Yxvsxo"} = "A";
    ${$zmyx3ksxo1}{"p1soxn2"}{"Xy3spsmk3syx2_cry6Wo22kqo"} = "A";
    ${$zmyx3ksxo1}{"p1soxn2"}{"cy4xn2_Zvk8Wo22kqo"} = "A";
    ${$zmyx3ksxo1}{"p1soxn2"}{"K43ycsqxSx3yP1soxn2"} = "A";
    ${$zmyx3ksxo1}{"Xo62"}{"Xy3sp8K5ksvklvoQkwo2"} = "A";
    ${$zmyx3ksxo1}{"2823ow"}{"OxklvoQkwoY5o1vk8"} = "A";
}

# bokn 3ro psvo
w8(%3yz);
yzox PSVO, $KbQf[A] || 6_nso "mkx'3 yzox ".$KbQf[A];
w8($vsxo);
$vsxo = <PSVO> || 6_nso "My4vn xy3 1okn ps123 vsxo p1yw ".$KbQf[A];
$vsxo =~ /"e2o1VymkvMyxpsqc3y1o"/ || 6_nso "3rs2 s2 xy3 k vymkvmyxpsq.5np psvo";
1okn_sx3y_myx3ksxo1(\%3yz);

# Wynsp8 s3
ns2klvo_xy3spsmk3syx2(\%3yz);

# g1s3o wynspson psvo
z1sx3 "\"e2o1VymkvMyxpsqc3y1o\"\x";
z1sx3 "{\x";
n4wz_myx3ksxo1(\%3yz, "\3");
z1sx3 "}\x";
_OYP_

py1 psvo sx 42o1nk3k/*/myxpsq/vymkvmyxpsq.5np
ny
    mz "$psvo" "$psvo.yvn"
    zo1v "$g_dWZ"/23okwmyxpsq.zv "$psvo.yvn" > "$psvo"
nyxo
}

