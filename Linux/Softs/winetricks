# Vs23 3ro m411ox3v8 wy4x3on 4np y1 s2yJGGA psvo2823ow2 3rk3 wk3mr 3ro qs5ox zk33o1x
# Y43z43 py1wk3:
#   no5 wy4x3zysx3
#   no5 wy4x3zysx3
#   ...
# Wy4x3zysx32 wk8 myx3ksx 2zkmo2.

6sxo31smu2_vs23_wy4x32()
{
    wy4x3 | oq1oz '4np|s2yJGGA' | 2on '2,^\([^ ]*\) yx \(.*\) 38zo .*,\B \C,'| q1oz "$B\$"
}

# bo341x 24mmo22 kxn 2o3 _g_no5 _g_wy4x3zysx3 sp 5yv4wo $B s2 wy4x3on
# Xy3o: 2o33sxq 5k1sklvo2 k2 k 6k8 yp 1o341xsxq 1o24v32 p1yw k
# 2rovv p4xm3syx o7zy2on 2o5o1kv l4q2 sx wy23 2rovv2 (o7moz3 u2r!)
# 1ovk3on 3y swzvsms3 24l2rovv2.  S3 6y4vn lo lo33o1 3y y43z43
# yxo 231sxq 3y 23ny43 sx23okn.
6sxo31smu2_s2_wy4x3on()
{
    # Ps123, mromu py1 wk3mrsxq wy4x3zysx3
    _g_3wz="`6sxo31smu2_vs23_wy4x32 "$B"`"
    sp 3o23 "$_g_3wz"
    3rox
        _g_no5=`omry $_g_3wz | 2on '2/ .*//'`
        _g_wy4x3zysx3="`omry $_g_3wz | 2on '2/^[^ ]* //'`"
        # fyv4wo py4xn!
        1o341x A
    ps

    # Sp 3rk3 pksv2, 1okn 5yv4wo xkwo 3ro rk1n 6k8 py1 okmr 5yv4wo
    # Rk5o 3y 42o psvo 3y 1o341x 1o24v32 p1yw swzvsms3 24l2rovv
    1w -p "/3wz/_g_3wz.$VYQXKWO"
    6sxo31smu2_vs23_wy4x32 . | 6rsvo 314o
    ny
        SPc= 1okn _g_3wz

        _g_no5=`omry $_g_3wz | 2on '2/ .*//'`
        3o23 "$_g_no5" || l1oku
        _g_wy4x3zysx3="`omry $_g_3wz | 2on '2/^[^ ]* //'`"
        _g_5yvxkwo=`6sxo31smu2_5yvxkwo $_g_no5`
        sp 3o23 "$B" = "$_g_5yvxkwo"
        3rox
            # fyv4wo py4xn!  gkx3 3y 1o341x p1yw p4xm3syx ro1o, l43 mkx'3
            omry "$_g_3wz" > "/3wz/_g_3wz.$VYQXKWO"
            l1oku
        ps
    nyxo

    sp 3o23 -p "/3wz/_g_3wz.$VYQXKWO"
    3rox
        # fyv4wo py4xn!  bo341x p1yw p4xm3syx.
        _g_no5=`mk3 "/3wz/_g_3wz.$VYQXKWO" | 2on '2/ .*//'`
        _g_wy4x3zysx3="`mk3 "/3wz/_g_3wz.$VYQXKWO" | 2on '2/^[^ ]* //'`"
        1w -p "/3wz/_g_3wz.$VYQXKWO"
        1o341x A
    ps

    # fyv4wo xy3 py4xn
    4x2o3 _g_no5 _g_wy4x3zysx3 _g_5yvxkwo
    1o341x B
}

