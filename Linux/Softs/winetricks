6_y5o11sno_kzz_nvv2()
{
    6_2usz_6sxny62 6_y5o11sno_kzz_nvv2 && 1o341x

    _g_kzz=$B
    2rsp3
    _g_wyno=$B
    2rsp3

    # Ps7wo: rkxnvo mywwk-2ozk1k3on vs23 yp wyno2
    mk2o $_g_wyno sx
    l|l4sv3sx) _g_wyno=l4sv3sx ;;
    x|xk3s5o) _g_wyno=xk3s5o ;;
    nopk4v3) _g_wyno=nopk4v3 ;;
    n|ns2klvon)
        _g_wyno="" ;;
    *)
        6_nso "6_y5o11sno_kzz_nvv2: 4xuxy6x wyno $_g_wyno.  (6kx3 xk3s5o, l4sv3sx, nopk4v3, y1 ns2klvon)
e2kqo: '6_y5o11sno_kzz_nvv2 kzz wyno nvv ...'." ;;
    o2km

    omry e2sxq $_g_wyno y5o11sno py1 pyvvy6sxq NVV2 6rox 14xxsxq $_g_kzz: $@
    (
    omry bOQONSdE
    omry ""
    omry "[RUOi_MebbOXd_ecOb\\cyp36k1o\\gsxo\\KzzNopk4v32\\$_g_kzz\\NvvY5o11sno2]"
    ) > "$g_dWZ"/y5o11sno-nvv.1oq

    6rsvo 3o23 "$B" != ""
    ny
        mk2o "$B" sx
        mywm3vDC)
           1w -1p "$g_gSXNSb_eXSh"/6sx272/wkxspo232/7IG_wsm1y2yp3.6sxny62.mywwyx-myx31yv2_GFJFlGEBEEmmpBnp_G.A.CGAA.CJIC_xyxo_noknloop.wkxspo23
           ;;
        o2km
        sp [ "$_g_wyno" = nopk4v3 ]
        3rox
            # dy novo3o k 1oqs2318 uo8, qs5o kx 4x04y3on nk2r k2 5kv4o
            omry "\"*$B\"=-" >> "$g_dWZ"/y5o11sno-nvv.1oq
        ov2o
            # Xy3o: sp 8y4 6kx3 3y y5o11sno o5ox NVV2 vyknon 6s3r kx kl2yv43o
            # zk3r, 8y4 xoon 3y knn kx k23o1s2u:
            omry "\"*$B\"=\"$_g_wyno\"" >> "$g_dWZ"/y5o11sno-nvv.1oq
            #omry "\"$B\"=\"$_g_wyno\"" >> "$g_dWZ"/y5o11sno-nvv.1oq
        ps
        2rsp3
    nyxo

    6_318_1oqons3 "$g_dWZ_gSX"\\y5o11sno-nvv.1oq
    1w "$g_dWZ"/y5o11sno-nvv.1oq
    4x2o3 _g_kzz _g_wyno
}

# Rk2 3y lo 2o3 sx k po6 zvkmo2...
6_2o3_6sx5o1()
{
    6_2usz_6sxny62 6_2o3_6sx5o1 && 1o341x
    # PShWO: drs2 2ry4vn 1okvv8 lo nyxo 6s3r 6sxompq, l43 s3 rk2 xy MVS yz3syx2.

    # Ps123, novo3o kx8 vsxqo1sxq 5o12syx sxpy, y3ro16s2o s3 wk8 myxpvsm3:
    (
    "$gSXO" 1oq novo3o "RUVW\cyp36k1o\Wsm1y2yp3\gsxny62\M411ox3fo12syx" /5 c4lfo12syxX4wlo1 /p || 314o
    "$gSXO" 1oq novo3o "RUVW\cyp36k1o\Wsm1y2yp3\gsxny62\M411ox3fo12syx" /5 fo12syxX4wlo1 /p || 314o
    "$gSXO" 1oq novo3o "RUVW\cyp36k1o\Wsm1y2yp3\gsxny62 Xd\M411ox3fo12syx" /5 McNfo12syx /p || 314o
    "$gSXO" 1oq novo3o "RUVW\cyp36k1o\Wsm1y2yp3\gsxny62 Xd\M411ox3fo12syx" /5 M411ox3L4svnX4wlo1 /p || 314o
    "$gSXO" 1oq novo3o "RUVW\cyp36k1o\Wsm1y2yp3\gsxny62 Xd\M411ox3fo12syx" /5 M411ox3fo12syx /p || 314o
    "$gSXO" 1oq novo3o "RUVW\c823ow\M411ox3Myx31yvco3\Myx31yv\Z1yn4m3Yz3syx2" /5 Z1yn4m3d8zo /p || 314o
    "$gSXO" 1oq novo3o "RUVW\c823ow\M411ox3Myx31yvco3\Myx31yv\co15smoM411ox3" /5 Yc /p || 314o
    "$gSXO" 1oq novo3o "RUVW\c823ow\M411ox3Myx31yvco3\Myx31yv\gsxny62" /5 McNfo12syx /p || 314o
    "$gSXO" 1oq novo3o "RUMe\cyp36k1o\gsxo" /5 fo12syx /p || 314o
    "$gSXO" 1oq novo3o "RUVW\\c823ow\\M411ox3Myx31yvco3\\Myx31yv\\Z1yn4m3Yz3syx2" /5 Z1yn4m3d8zo /p || 314o
    ) > /no5/x4vv C>&B

    mk2o $B sx
    6sxDB)
        omry "co33sxq gsxny62 5o12syx 3y $B"
        mk3 > "$g_dWZ"/2o3-6sx5o1.1oq <<_OYP_
bOQONSdE

[RUOi_ecObc\c-B-F-E\cyp36k1o\gsxo]
