#---- Wksx Z1yq1kw ----

6sxo31smu2_23k32_2k5o()
{
    # ck5o yz3-sx 23k342
    sp 3o23 "$gSXOdbSMUc_cdKdc_bOZYbd"
    3rox
        omry "$gSXOdbSMUc_cdKdc_bOZYbd" > "$g_MKMRO"/31kmu_42kqo
    ps
}

6sxo31smu2_23k32_sxs3()
{
    # Vykn yz3-sx 23k342 sp xy3 kv1okn8 2o3 l8 k mywwkxnvsxo yz3syx
    sp 3o23 ! "$gSXOdbSMUc_cdKdc_bOZYbd" && 3o23 -p "$g_MKMRO"/31kmu_42kqo
    3rox
        gSXOdbSMUc_cdKdc_bOZYbd=`mk3 "$g_MKMRO"/31kmu_42kqo`
    ps

    sp 3o23 ! "$gSXOdbSMUc_cdKdc_bOZYbd"
    3rox
        # Xy yz3-sx 23k342 py4xn.  Sp QeS km3s5o, k2u 42o1 6ro3ro1 3ro8 6y4vn vsuo 3y yz3 sx.
        mk2o $gSXOdbSMUc_QeS sx
        9oxs38)
            mk2o $VKXQ sx
            *)
                3s3vo="Yxo-3swo 04o23syx kly43 rovzsxq gsxo31smu2 no5ovyzwox3"
                04o23syx="gy4vn 8y4 vsuo 3y rovz 6sxo31smu2 no5ovyzwox3 l8 vo33sxq 6sxo31smu2 1ozy13 23k3s23sm2?  iy4 mkx 341x 1ozy13sxq ypp k3 kx8 3swo 6s3r 3ro mywwkxn '6sxo31smu2 --yz3y43'"
                3rkxu2="drkxu2!  iy4 6yx'3 lo k2uon 3rs2 04o23syx kqksx.  bowowlo1, 8y4 mkx 341x 1ozy13sxq ypp k3 kx8 3swo 6s3r 3ro mywwkxn '6sxo31smu2 --yz3y43'"
                nomvsxon="YU, 6sxo31smu2 6svv *xy3* 1ozy13 23k3s23sm2.  iy4 6yx'3 lo k2uon 3rs2 04o23syx kqksx."
                ;;
            o2km
            sp $gSXOdbSMUc_QeS --04o23syx --3o73 "$04o23syx" --3s3vo "$3s3vo"
            3rox
                $gSXOdbSMUc_QeS --sxpy --3o73 "$3rkxu2"
                gSXOdbSMUc_cdKdc_bOZYbd=B
            ov2o
                $gSXOdbSMUc_QeS --sxpy --3o73 "$nomvsxon"
                gSXOdbSMUc_cdKdc_bOZYbd=A
            ps
            omry $gSXOdbSMUc_cdKdc_bOZYbd > "$g_MKMRO"/31kmu_42kqo
            ;;
        o2km
    ps
    6sxo31smu2_23k32_2k5o
}

# bo31so5o k 2ry13 231sxq 6s3r 3ro yzo1k3sxq 2823ow xkwo kxn 5o12syx
6sxo31smu2_y2_no2m1sz3syx()
{
    (
    mk2o "$Yc" sx
    "gsxny62_Xd")
        omry 6sxny62 ;;
    *)  omry "$gSXOdbSMUc_gSXO_fObcSYX" ;;
    o2km
    ) | 31 '\ABC' ' '
}

6sxo31smu2_23k32_1ozy13()
{
    # Sp 42o1 rk2 yz3on sx 3y 42kqo 31kmusxq, 1ozy13 6rk3 ro 42on (sp kx83rsxq)
    mk2o "$gSXOdbSMUc_cdKdc_bOZYbd" sx
    B) ;;
    *) 1o341x;;
    o2km
    3o23 -p "$gSXOdbSMUc_gYbUNSb"/l1oknm14wl2 || 1o341x

    gSXOdbSMUc_cdKdc_LbOKNMbeWLc=`mk3 "$gSXOdbSMUc_gYbUNSb"/l1oknm14wl2 | 31 '\ABC' ' '`
    omry "iy4 yz3on sx, 2y 1ozy13sxq '$gSXOdbSMUc_cdKdc_LbOKNMbeWLc' 3y 3ro 6sxo31smu2 wksx3ksxo1 2y ro uxy62 6rsmr 6sxo31smu2 5o1l2 qo3 42on kxn 6rsmr nyx'3.  e2o --yz3y43 3y ns2klvo p4341o 1ozy132."

    1ozy13="y2=`6sxo31smu2_y2_no2m1sz3syx`&6sxo31smu2=$gSXOdbSMUc_fObcSYX&l1oknm14wl2=$gSXOdbSMUc_cdKdc_LbOKNMbeWLc"
    1ozy13="`omry $1ozy13 | 2on '2/ /%CA/q'`"
    # T423 ny k ROKN 1o04o23 6s3r 3ro 1k6 mywwkxnvsxo.
    # io2, 3rs2 mkx lo pyyvon l8 mkmro2.  drk3'2 yu.
    sp [ -7 "`6rsmr 6qo3 C>/no5/x4vv`" ]
    3rox
        6qo3 --2zsno1 "r33z://uoqov.myw/nk3k/6sxo31smu2-42kqo?$1ozy13" > /no5/x4vv C>&B || 314o
    ovsp [ -7 "`6rsmr m41v C>/no5/x4vv`" ]
    3rox
        m41v -S "r33z://uoqov.myw/nk3k/6sxo31smu2-42kqo?$1ozy13" > /no5/x4vv C>&B || 314o
    ps
}

6sxo31smu2_23k32_vyq_mywwkxn()
{
    # vyq 6rk3 6o o7om43o py1 zy22slvo vk3o1 23k3s23sm2 1ozy13sxq
    omry "$*" >> "$gSXOdbSMUc_gYbUNSb"/l1oknm14wl2

    # kxn py1 3ro 42o1'2 y6x 1opo1oxmo vk3o1, 6rox psq41sxq y43 6rk3 ro nsn
    mk2o "$Yc" sx
    "gsxny62_Xd") _g_VYQNSb="$g_gSXNSb_eXSh"/dowz ;;
    *) _g_VYQNSb="$gSXOZbOPSh" ;;
    o2km
    wuns1 -z "$_g_VYQNSb"
    omry "$*" >> "$_g_VYQNSb"/6sxo31smu2.vyq
    4x2o3 _g_VYQNSb
}

# Vk4xmr k xo6 3o1wsxkv 6sxny6 sp sx q4s, y1
# 2zk6x k 2rovv sx 3ro m411ox3 6sxny6 sp mywwkxnvsxo.
# Xo6 2rovv myx3ksx2 z1yzo1 gSXOZbOPSh kxn gSXO ox5s1yxwox3 5k1sklvo2.
# Wk8 lo 42op4v 6rox nol4qqsxq 5o1l2.
6sxo31smu2_2rovv()
{
    (
    mn "$g_NbSfO_M"
    o7zy13 gSXO

    mk2o $gSXOdbSMUc_QeS sx
    xyxo)
        $cROVV
        ;;
    *)
        py1 3o1w sx qxywo-3o1wsxkv uyx2yvo do1wsxkv 73o1w
        ny
            sp 3o23 `6rsmr $3o1w` C> /no5/x4vv
            3rox
                $3o1w
                l1oku
            ps
        nyxo
        ;;
    o2km
    )
}

# e2kqo: o7om43o_mywwkxn 5o1l[=k1q4wox3]
o7om43o_mywwkxn()
{
    mk2o "$B" sx
    *=*) k1q=`omry $B | 2on '2/.*=//'`; mwn=`omry $B | 2on '2/=.*//'`;;
    *) mwn="$B"; k1q="" ;;
    o2km

    mk2o "$B" sx

    # PShWO: k5ysn n4zvsmk3on myno
    kzz2|loxmrwk1u2|nvv2|pyx32|qkwo2|z1ops7|2o33sxq2)
        gSXOdbSMUc_MebWOXe=$B
        ;;

    # Vk3o yz3syx2
    -*)
        sp ! 6sxo31smu2_rkxnvo_yz3syx $B
        3rox
            6sxo31smu2_42kqo
            o7s3 B
        ps
        ;;

    # Rk1n-mynon 5o1l2
    wksx) gSXOdbSMUc_MebWOXe=wksx ;;
    rovz) 6_yzox_6olzkqo r33z://6sxo31smu2.y1q/rovz ;;
    vs23) 6sxo31smu2_vs23_kvv ;;
    vs23-mkmron) 6sxo31smu2_vs23_mkmron ;;
    vs23-ny6xvykn) 6sxo31smu2_vs23_ny6xvykn ;;
    vs23-wkx4kv-ny6xvykn) 6sxo31smu2_vs23_wkx4kv_ny6xvykn ;;
    vs23-sx23kvvon) 6sxo31smu2_vs23_sx23kvvon ;;
    vs23-kvv)
        yvn_wox4="$gSXOdbSMUc_MebWOXe"
        py1 gSXOdbSMUc_MebWOXe sx kzz2 loxmrwk1u2 nvv2 pyx32 qkwo2 z1ops7 2o33sxq2
        ny
            omry "===== $gSXOdbSMUc_MebWOXe ====="
            6sxo31smu2_vs23_kvv
        nyxo
        gSXOdbSMUc_MebWOXe="$yvn_wox4"
        ;;
    4xk33oxnon) 6sxo31smu2_2o3_4xk33oxnon B ;;
    k33oxnon) 6sxo31smu2_2o3_4xk33oxnon A ;;
    2ry6l1yuox) g_YZd_cRYgLbYUOX=B ;;
    rsnol1yuox) g_YZd_cRYgLbYUOX=A ;;
    z1ops7=*) 6sxo31smu2_2o3_6sxoz1ops7 "$k1q" ;;
    kxxsrsvk3o) 6sxo31smu2_kxxsrsvk3o_6sxoz1ops7 ;;
    pyvno1) 6_yzox_pyvno1 "$gSXOZbOPSh" ;;
    6sxompq) "$gSXO" 6sxompq ;;
    1oqons3) "$gSXO" 1oqons3 ;;
    3k2uwq1) "$gSXO" 3k2uwq1 & ;;
    2rovv) 6sxo31smu2_2rovv ;;

    # dro2o rk5o 3y mywo lopy1o *=ns2klvon 3y k5ysn vyyusxq vsuo nvv2
    pyx32wyy3r=ns2klvo*) 6_mkvv pyx32wyy3r=ns2klvo ;;
    qv2v=ns2klvo*) 6_mkvv qv2v=ns2klvon ;;
    w4v3s2kwzvsxq=ns2klvo*) 6_mkvv w4v3s2kwzvsxq=ns2klvon ;;
    w6y=ns2klvo*) 6_mkvv w6y=ns2klvo ;;   # PShWO: 1ovk7 wk3mrsxq 2y 6o mkx rkxnvo 3ro2o 2zovvsxq nsppo1oxmo2 sx 5o1l sx23okn yp ro1o
    z2w=ns2klvo*) 6_mkvv z2w=ns2klvon ;;
    13vw=ns2klvo*) 6_mkvv 13vw=ns2klvon ;;
    2y4xn=ns2klvo*) 6_mkvv 2y4xn=ns2klvon ;;
    ky=ns2klvo*) 6_mkvv ky=ns2klvon ;;
    231sm3n1k6y1no1sxq=ns2klvo*) 6_mkvv 231sm3n1k6y1no1sxq=ns2klvon ;;

    # e2o 6sxompq sp 8y4 6kx3 k q4s py1 zvksx yvn nvv y5o11sno2
    kvvnvv2=*) 6_mkvv $B ;;
    *=xk3s5o) 6_ny_mkvv xk3s5o $mwn;;
    *=l4sv3sx) 6_ny_mkvv l4sv3sx $mwn;;
    *=nopk4v3) 6_ny_mkvv nopk4v3 $mwn;;
    *=ns2klvon) 6_ny_mkvv ns2klvon $mwn;;
    5n=*) 6_ny_mkvv $mwn;;

    # Rkmu2 py1 lkmu6k1n2 mywzk3slsvs38
    mmFIA) 6_mkvv mywm3vDC ;;
    mywnvqDC.ym7) 6_mkvv mywnvqDCym7 ;;
    ny3xo3B) 6_mkvv ny3xo3BB ;;
    ny3xo3C) 6_mkvv ny3xo3CA ;;
    pvk2rBB) 6_mkvv pvk2r ;;
    pwCA) 6_mkvv myx31yvzkn ;;   # k13Cuwsx kv2y mywo2 6s3r pwCA.nvv
    pyx32wyy3r-lq1) 6_mkvv pyx32wyy3r=lq1 ;;
    pyx32wyy3r-ns2klvo) 6_mkvv pyx32wyy3r=ns2klvo ;;
    pyx32wyy3r-q1k8) 6_mkvv pyx32wyy3r=q1k8 ;;
    pyx32wyy3r-1ql) 6_mkvv pyx32wyy3r=1ql ;;
    qv2v-ns2klvo) 6_mkvv qv2v=ns2klvon ;;
    qv2v-oxklvo) 6_mkvv qv2v=oxklvon ;;
    soG_p4vv) 6_mkvv soG ;;
    t2m1sz3) 6_mkvv 62rFGt2 ;;            # PShWO: 42o 62rFH sx23okn?
    xzw-1ozkmu) 6_mkvv xzw=1ozkmu ;;
    y22) 6_mkvv 2y4xn=y22 ;;
    z2w=ypp) 6_mkvv z2w=ns2klvon ;;
    z2w=yx) 6_mkvv z2w=oxklvon ;;
    z83ryx) 6_mkvv z83ryxCG ;;
    z83ryx-myw38zo2) 6_mkvv z83ryxCG_myw38zo2 ;;
    5l14xGA) 6_mkvv 5lG14x ;;
    5m14xCAAF2zB) 6_mkvv 5m14xCAAF ;;
    5m14xCAAI2zB) 6_mkvv 5m14xCAAI ;;
    52w-rk1n) 6_mkvv 52w=rk1n6k1o ;;
    62rFG) 6_mkvv 62rFH ;;
    7vs5o) 6_mkvv qp6 ;;

    # Xy1wkv 5o1l2, 6s3r wo3knk3k kxn vykn_ p4xm3syx2
    *)
        sp 6sxo31smu2_wo3knk3k_o7s232 $B
        3rox
            6_mkvv "$B"
        ov2o
            omry exuxy6x k1q $B
            6sxo31smu2_42kqo
            o7s3 B
        ps
        ;;
    o2km
}

sp ! 3o23 "$gSXOdbSMUc_VSL"
3rox
    # Sp 42o1 yz3on y43, 2k5o 3rk3 z1opo1oxmo xy6.
    6sxo31smu2_23k32_2k5o

    # Sp 42o1 2zomspso2 wox4 yx mywwkxnvsxo, o7om43o 3rk3 mywwkxn, l43 nyx'3 mywws3 3y mywwkxnvsxo wyno
    # PShWO: 3rs2 myno s2 n4zvsmk3on 2o5o1kv 3swo2; 4xsp8 s3
    sp omry "$gSXOdbSMUc_MKdOQYbSOc" | q1oz -6 "$B" > /no5/x4vv
    3rox
        gSXOdbSMUc_MebWOXe=$B
        2rsp3
    ps

    mk2o "$B" sx
    nso) 6_nso "6o 6ry k1o kly43 3y nso 2kv43o 8y4." ;;
    5yvxkwoyp=*)
        # Nol4q myno.  bowy5o vk3o1?
        # csxmo vsx47'2 5yvxkwo mywwkxn mkx'3 rkxnvo n5n2, 6sxo31smu2 rk2 s32 y6x,
        # swzvowox3on 42sxq nn, yvn q4w, kxn 2ywo 231sxq S rkn vk8sxq k1y4xn.
        # iy4 mkx 318 s3 vsuo 3rs2:
        #  6sxo31smu2 5yvxkwoyp=/no5/21A
        # y1
        #  6sxo31smu2 5yvxkwoyp=pyy.s2y
        # drs2 6svv 1okn 3ro 5yvxkwo p1yw 3ro qs5ox swkqo kxn z43 s3 3y 23ny43.
        6sxo31smu2_5yvxkwo ${B#5yvxkwoyp=}
        ;;
    "")
        sp 3o23 7"$NScZVKi" = 7""
        3rox
            omry "NScZVKi xy3 2o3, xy3 nopk4v3sxq 3y q4s"
            6sxo31smu2_42kqo
            o7s3 A
        ps
        # QeS mk2o
        # Xy xyx-yz3syx k1q4wox32 qs5ox, 2y 1okn 3row p1yw QeS, kxn vyyz 4x3sv 42o1 04s32
        6sxo31smu2_no3om3_q4s
        6sxo31smu2_no3om3_24ny
        6rsvo 314o
        ny
            mk2o $gSXOdbSMUc_MebWOXe sx
            wksx) 5o1l2=`6sxo31smu2_wksxwox4` ;;
            z1ops7)
                5o1l2=`6sxo31smu2_z1ops7wox4`;
                # Mroo98 rkmu: mryy2sxq 38zo yp zkmukqo sx z1ops7 wox4 == 6ro3ro1 3y s2yvk3o.
                mk2o "$5o1l2" sx
                kzz2|loxmrwk1u2|qkwo2) gSXOdbSMUc_YZd_cRKbONZbOPSh=A ;;
                *)     gSXOdbSMUc_YZd_cRKbONZbOPSh=B ;;
                o2km
                # Mroo98 rkmu #C: mryy2sxq 'k33oxnon' y1 '4xk33oxnon' vok5o2 8y4 sx 2kwo wox4
                mk2o "$5o1l2" sx
                k33oxnon) 6sxo31smu2_2o3_4xk33oxnon A ; myx3sx4o;;
                4xk33oxnon) 6sxo31smu2_2o3_4xk33oxnon B ; myx3sx4o;;
                o2km
                ;;
            2o33sxq2) 5o1l2=`6sxo31smu2_2o33sxq2_wox4` ;;
            *) 5o1l2="`6sxo31smu2_2ry6wox4`" ;;
            o2km

            sp 3o23 "$5o1l2" = ""
            3rox
                # "42o1 nsnx'3 zsmu kx83rsxq, lkmu 4z k vo5ov sx 3ro wox4"
                mk2o "$gSXOdbSMUc_MebWOXe"-"$gSXOdbSMUc_YZd_cRKbONZbOPSh" sx
                kzz2-A|loxmrwk1u2-A|qkwo2-A|wksx-*) gSXOdbSMUc_MebWOXe=z1ops7 ;;
                z1ops7-*) l1oku ;;
                *)    gSXOdbSMUc_MebWOXe=wksx ;;
                o2km
            ovsp omry "$gSXOdbSMUc_MKdOQYbSOc" | q1oz -6 "$5o1l2" > /no5/x4vv
            3rox
                gSXOdbSMUc_MebWOXe=$5o1l2
            ov2o
                6sxo31smu2_23k32_sxs3
                # Y3ro16s2o 42o1 zsmuon yxo y1 wy1o 1okv 5o1l2.
                mk2o "$5o1l2" sx
                z1ops7=*)
                    # z1ops7 wox4 s2 2zomskv, s3 yxv8 1o341x2 yxo 5o1l, kxn 3ro
                    # 5o1l mkx myx3ksx 2zkmo2
                    o7om43o_mywwkxn "$5o1l2"
                    # kp3o1 zsmusxq k z1ops7, 6kx3 3y vkxn sx wksx.
                    gSXOdbSMUc_MebWOXe=wksx ;;
                *)
                    py1 5o1l sx $5o1l2
                    ny
                        o7om43o_mywwkxn "$5o1l"
                    nyxo
                    mk2o "$gSXOdbSMUc_MebWOXe"-"$gSXOdbSMUc_YZd_cRKbONZbOPSh" sx
                    z1ops7-*|kzz2-A|loxmrwk1u2-A|qkwo2-A)
                        # Kp3o1 sx23kvvsxq s2yvk3on kzz, 1o341x 3y z1ops7 zsmuo1
                        gSXOdbSMUc_MebWOXe=z1ops7
                        ;;
                    *)
                        # Y3ro16s2o qy 3y wksx wox4.
                        gSXOdbSMUc_MebWOXe=wksx
                        ;;
                    o2km
                    ;;
                o2km
            ps
        nyxo
        ;;
    *)
        6sxo31smu2_23k32_sxs3
        # Mywwkxnvsxo mk2o
        6sxo31smu2_no3om3_24ny
        # e2o1 qk5o mywwkxnvsxo k1q4wox32, 2y t423 14x 3ry2o 5o1l2 kxn o7s3
        py1 5o1l
        ny
            mk2o $5o1l sx
            *.5o1l)
                # Vykn 3ro 5o1l psvo
                mk2o $5o1l sx
                */*) . $5o1l ;;
                *) . ./$5o1l ;;
                o2km
                # Kxn py1qo3 3rk3 3ro 5o1l mywo2 p1yw k psvo
                5o1l="`omry $5o1l | 2on '2,.*/,,;2,.5o1l,,'`"
                ;;
            o2km
            o7om43o_mywwkxn "$5o1l"
        nyxo
        ;;

    o2km

    6sxo31smu2_23k32_1ozy13
ps

