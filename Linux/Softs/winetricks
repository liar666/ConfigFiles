    z1sx3p %2 "$gSXOZbOPSh"/ny2no5smo2/m:
    # Mrkxqo lkmu2vk2ro2 3y 2vk2ro2
    omry $_g_6sxz | 2on '2,\\,/,q'
}

# Myx5o13 lo36oox exs7 zk3r kxn gsxny62 zk3r
