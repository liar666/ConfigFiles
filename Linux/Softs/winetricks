        ps
    ps

    sp 6_6y1uk1y4xn_6sxo_l4q CGFID "Sx23kvvsxq xk3s5o nDn7J_DG"
    3rox
        6_mkvv nDn7J_DG
    ps

    # PShWO: psq41o y43 6r8 3ro2o o7om43klvo2 nyx'3 o7s3, kxn ny k z1yzo1 6y1uk1y4xn y1 ps7
    2vooz BA
    sp z2 k4q76 | q1oz -s o7o | oq1oz '6sxowox4l4svno1.o7o|2o34z.o7o|ZxuL231K.o7o | oq1oz -5 oq1oz'
