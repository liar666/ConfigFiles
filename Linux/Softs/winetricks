vykn_vrz_nowy()
{
    mk2o "$VKXQ" sx
    *edP-I*|*43pI*) ;;
    *)
        6_6k1x "drs2 sx23kvvo1 pksv2 sx xyx-43p-I vymkvo2.  Nysxq 'o7zy13 VKXQ=ox_ec.edP-I'."
        VKXQ=ox_ec.edP-I
        o7zy13 VKXQ
        ;;
    o2km

    6_ny6xvykn "r33z://23k3sm.usn26l.myw/voqyrk118zy33o1qkwo/VOQYRk118Zy33o1NOWY.o7o" llAkDAknJkHmmFBmIAoBllBpDoomCCoGmmmBkHAG

    mn "$g_MKMRO/$g_ZKMUKQO"
    6_kru_ny "
        co3ds3voWk3mrWyno, C
        14x, VOQYRk118Zy33o1NOWY.o7o
        6sx6ks3, VOQY, vkxq4kqo
        sp ( 6_yz3_4xk33oxnon > A ) {
            myx31yvmvsmu, L433yxB
            6sx6ks3, VOQY, Vsmox2o
            myx31yvmvsmu, L433yxB
            myx31yvmvsmu, L433yxC
            6sx6ks3, VOQY, sx23kvvk3syx wo3ryn
            myx31yvmvsmu, L433yxC
        }
        6sx6ks3, VOQY, Psxs2r
        sp ( 6_yz3_4xk33oxnon > A )
            myx31yvmvsmu, L433yxB

        6sx6ks3mvy2o, VOQY, Psxs2r
    "

    sp 6_6y1uk1y4xn_6sxo_l4q CDDJH
    3rox
        6_6k1x "Sp 2y4xn 23433o12, 318 26s3mrsxq 3y Ycc 2y4xn sx 6sxompq (6sxo31smu2 2y4xn=y22)"
    ps

    # gy1u k1y4xn vymkvo s224o2 l8 28wvsxusxq 3ro kzz'2 ns1om3y18 3y xy3 rk5o k p4xx8 mrk1
    # gyx'3 1okvv8 6y1u yx m8q6sx, l43 3rk3'2 yu.
    mn "$g_ZbYQbKWc_hIG_eXSh/gL Qkwo2"
    vx -2 VOQY*Rk118\ Zy33o1*NOWY VOQY_Rk118_Zy33o1_NOWY

    6_nomvk1o_o7o "$g_ZbYQbKWc_hIG_gSX\\gL Qkwo2\\VOQY_Rk118_Zy33o1_NOWY" "VOQYRk118Zy33o1NOWY.o7o"
}

