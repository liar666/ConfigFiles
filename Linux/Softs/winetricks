# bo341x2 314o sp qs5ox 5o1l rk2 loox mkmron
# iy4 w423 rk5o kv1okn8 vyknon s32 wo3knk3k lopy1o mkvvsxq
6sxo31smu2_s2_mkmron()
{
    # PShWO: kv2y mromu psvoC... sp qs5ox
    _g_zk3r="$g_MKMRO/$B/$psvoB"
    mk2o "$_g_zk3r" sx
    *..*)
        # bowy5o /pyy/.. 2y 5o1l2 3rk3 nyx'3 rk5o 3ros1 y6x mkmro ns1om3y1so2
        # mkx 1opo1 3y 2slvsxq2
        _g_zk3r="`omry $_g_zk3r | 2on '2,/[^/]*/\.\.,,'`"
        ;;
    o2km
    sp 3o23 -p "$_g_zk3r"
    3rox
        4x2o3 _g_zk3r
        1o341x A
    ps
    4x2o3 _g_zk3r
    1o341x B
}

# bo341x2 314o sp qs5ox 5o1l rk2 loox sx23kvvon
# iy4 w423 rk5o kv1okn8 vyknon s32 wo3knk3k lopy1o mkvvsxq
6sxo31smu2_s2_sx23kvvon()
{
    4x2o3 _g_psvo _g_psvo_4xs7
    sp 3o23 "$sx23kvvon_o7oB"
    3rox
        _g_psvo="$sx23kvvon_o7oB"
    ovsp 3o23 "$sx23kvvon_psvoB"
    3rox
        _g_psvo="$sx23kvvon_psvoB"
    ov2o
        1o341x B  # xy3 sx23kvvon
    ps

    mk2o "$Yc" sx
    gsxny62_Xd)
        # Yx gsxny62, 3ro1o'2 xy 6sxoz1ops7, t423 mromu sp psvo'2 3ro1o
        _g_psvo_4xs7="`6_zk3rmyx5 -4 "$_g_psvo"`"
        sp 3o23 -p "$_g_psvo_4xs7"
        3rox
            4x2o3 _g_psvo _g_psvo_4xs7 _g_z1ops7
            1o341x A  # sx23kvvon
        ps
        ;;
    *)
        # Mywz43o 6sxoz1ops7 py1 3rs2 kzz
        mk2o "$mk3oqy18"-"$gSXOdbSMUc_YZd_cRKbONZbOPSh" sx
        kzz2-A|loxmrwk1u2-A|qkwo2-A)
            _g_z1ops7="$g_ZbOPShOc_bYYd/$B"
            ;;
        *)
            _g_z1ops7="$gSXOZbOPSh"
            ;;
        o2km
        sp 3o23 -n "$_g_z1ops7/ny2no5smo2"
        3rox
          # '6sxH 5m14xCAAF' m1ok3o2 nspp1ox3 psvo 3rkx '6sx7z 5m14xCAAF'
          # 2y vo3 s3 2zomsp8 w4v3szvo, 2ozk1k3on l8 |
          _g_SPc="$SPc"
          SPc='|'
          py1 _g_psvo_ sx $_g_psvo
          ny
            _g_psvo_4xs7="`gSXOZbOPSh="$_g_z1ops7" 6_zk3rmyx5 -4 "$_g_psvo_"`"
            sp 3o23 -p "$_g_psvo_4xs7" && ! q1oz -0 "gsxo zvkmoryvno1 NVV" "$_g_psvo_4xs7"
            3rox
                SPc="$_g_SPc"
                4x2o3 _g_psvo _g_psvo_ _g_psvo_4xs7 _g_z1ops7 _g_SPc
                1o341x A  # sx23kvvon
            ps
          nyxo
         SPc="$_g_SPc"
        ps
        ;;
    o2km
    4x2o3 _g_psvo _g_z1ops7  # voku _g_psvo_4xs7 py1 mkvvo1.  S2 3rs2 6s2o?
    4x2o3 _g_SPc _g_psvo_
    1o341x B  # xy3 sx23kvvon
}

# Vs23 5o1l2 6rsmr k1o kv1okn8 p4vv8 mkmron vymkvv8
6sxo31smu2_vs23_mkmron()
{
    py1 _g_wo3knk3kpsvo sx "$gSXOdbSMUc_WOdKNKdK"/*/*.5k12
    ny
        # e2o k 24l2rovv 3y k5ysn z433sxq wo3knk3k sx qvylkv 2zkmo
        # Sp 3rs2 s2 3yy 2vy6, 6o mkx 4x2o3 uxy6x wo3knk3k l8 rkxn
        (
        myno=`6sxo31smu2_wo3knk3k_lk2oxkwo "$_g_wo3knk3kpsvo"`
        . "$_g_wo3knk3kpsvo"
        sp 6sxo31smu2_s2_mkmron $myno
        3rox
            omry $myno
        ps
        )
    nyxo | 2y13
    4x2o3 _g_wo3knk3kpsvo
}

# Vs23 5o1l2 6rsmr k1o k43ywk3smkvv8 ny6xvyknklvo, 1oqk1nvo22 yp 6ro3ro1 3ro8'1o mkmron 8o3
6sxo31smu2_vs23_ny6xvykn()
{
    mn "$gSXOdbSMUc_WOdKNKdK"
    q1oz -v 'wonsk=.ny6xvykn' */*.5k12 | 2on '2,.*/,,;2/\.5k12//' | 2y13 -4
}

# Vs23 5o1l2 6rsmr k1o ny6xvyknklvo 6s3r 42o1 sx3o15ox3syx, 1oqk1nvo22 yp 6ro3ro1 3ro8'1o mkmron 8o3
6sxo31smu2_vs23_wkx4kv_ny6xvykn()
{
    mn "$gSXOdbSMUc_WOdKNKdK"
    q1oz -v 'wonsk=.wkx4kv_ny6xvykn' */*.5k12 | 2on '2,.*/,,;2/\.5k12//' | 2y13 -4
}

6sxo31smu2_vs23_sx23kvvon()
{
    (
    # T4wz 3r1y4qr k my4zvo ryyz2 3y o5kv4k3o 3ro 5o1l2 sx kvzrklo3smkv y1no1
    # K224wo 3rk3 xy psvoxkwo myx3ksx2 '|'
    mn "$gSXOdbSMUc_WOdKNKdK"
    py1 _g_wo3knk3kpsvo sx `v2 */*.5k12 | 2on '2,^\(.*\)/,\B|,' | 2y13 -3\| -u +C | 31 '|' /`
    ny
        # e2o k 24l2rovv 3y k5ysn z433sxq wo3knk3k sx qvylkv 2zkmo
        # Sp 3rs2 s2 3yy 2vy6, 6o mkx 4x2o3 uxy6x wo3knk3k l8 rkxn
        (
        myno=`6sxo31smu2_wo3knk3k_lk2oxkwo "$_g_wo3knk3kpsvo"`
        . "$_g_wo3knk3kpsvo"
        sp 6sxo31smu2_s2_sx23kvvon $myno
        3rox
            omry $myno
        ps
        )
    nyxo
    )
    4x2o3 _g_wo3knk3kpsvo
}

# Rovzo1 py1 knnsxq k 231sxq 3y k vs23 yp pvkq2
6sxo31smu2_kzzoxn_3y_pvkq2()
{
    sp 3o23 "$pvkq2"
    3rox
        pvkq2="$pvkq2,"
    ps
    pvkq2="${pvkq2}$B"
}

# Vs23 kvv 5o1l2 sx mk3oqy18 gSXOdbSMUc_MebWOXe 5o1ly2ov8
# Py1wk3 s2 "5o1l  3s3vo  (z4lvs2ro1, 8ok1) [pvkq2]"
6sxo31smu2_vs23_kvv()
{
    # Xy3o: nyrBCD 1ovso2 yx '6sxo31smu2 vs23' 3y vs23 wksx wox4 mk3oqy1so2
    mk2o $gSXOdbSMUc_MebWOXe sx
    z1ops7|wksx) omry "$gSXOdbSMUc_MKdOQYbSOc" | 31 ' ' '\ABC' ; 1o341x;;
    o2km

    mk2o $VKXQ sx
    nk*) _g_mkmron="mkmron"   ; _g_ny6xvykn="ukx rox3o2"    ;;
    no*) _g_mkmron="qomkmron" ; _g_ny6xvykn="ro14x3o1vknlk1";;
    zv*) _g_mkmron="9k1mrs6s9y6kxo"   ; _g_ny6xvykn="ny zyl1kxsk"  ;;
    4u*) _g_mkmron="ЄпђЈмкЇЈ"   ; _g_ny6xvykn="ЁкмкЇькЀэмкЇп"  ;;
    *)   _g_mkmron="mkmron"   ; _g_ny6xvykn="ny6xvyknklvo"  ;;
    o2km

    py1 _g_wo3knk3kpsvo sx "$gSXOdbSMUc_WOdKNKdK"/$gSXOdbSMUc_MebWOXe/*.5k12
    ny
        # e2o k 24l2rovv 3y k5ysn z433sxq wo3knk3k sx qvylkv 2zkmo
        # Sp 3rs2 s2 3yy 2vy6, 6o mkx 4x2o3 uxy6x wo3knk3k l8 rkxn
        (
        myno=`6sxo31smu2_wo3knk3k_lk2oxkwo "$_g_wo3knk3kpsvo"`
        . "$_g_wo3knk3kpsvo"

        # Mywz43o mkmron kxn ny6xvyknklvo pvkq2
        pvkq2=""
        3o23 "$wonsk" = "ny6xvykn" && 6sxo31smu2_kzzoxn_3y_pvkq2 "$_g_ny6xvykn"
        6sxo31smu2_s2_mkmron $myno   && 6sxo31smu2_kzzoxn_3y_pvkq2 "$_g_mkmron"
        3o23 "$pvkq2" && pvkq2="[$pvkq2]"

        sp ! 3o23 "$8ok1" && ! 3o23 "$z4lvs2ro1"
        3rox
            z1sx3p "%-CE2 %2 %2\x" $myno "$3s3vo" "$pvkq2"
        ov2o
            z1sx3p "%-CE2 %2 (%2, %2) %2\x" $myno "$3s3vo" "$z4lvs2ro1" "$8ok1" "$pvkq2"
        ps
        )
    nyxo
    4x2o3 _g_mkmron _g_wo3knk3kpsvo
}

# Kly13 sp 42o1 nyo2x'3 y6x 3ro qs5ox ns1om3y18 (y1 s32 zk1ox3, sp s3 nyo2x'3 o7s23 8o3)
6sxo31smu2_nso_sp_42o1_xy3_ns1y6xo1()
{
    sp 3o23 -n "$B"
    3rox
        _g_mromuns1="$B"
    ov2o
        # ps7wo: 04y3sxq z1ylvow?
        _g_mromuns1=`ns1xkwo "$B"`
    ps
    _g_x42o1=`sn -4`
    _g_xy6xo1=`v2 -v -x -n -V "$_g_mromuns1" | k6u '{z1sx3 $D}'`
    sp 3o23 7$_g_x42o1 != 7$_g_xy6xo1
    3rox
        6_nso "iy4 (`sn -4x`) nyx'3 y6x $_g_mromuns1.  Nyx'3 14x 3rs2 3yyv k2 kxy3ro1 42o1!"
    ps
}

# coo
# r33z://666.omwk-sx3o1xk3syxkv.y1q/z4lvsmk3syx2/psvo2/OMWK-cd/Omwk-BBJ.znp (s2yJGGA)
# r33z://666.omwk-sx3o1xk3syxkv.y1q/z4lvsmk3syx2/psvo2/OMWK-cd/Omwk-BGH.znp
# r33z://666.y23k.y1q/2zom2/znp/4npBAC.znp
# r33z://666.omwk-sx3o1xk3syxkv.y1q/z4lvsmk3syx2/3omr1ozy132/O-db-AHB.r3w

# e2kqo: 1okn_l83o2 ypp2o3 my4x3 no5smo
6sxo31smu2_1okn_l83o2()
{
    nn 23k342=xy7po1 sp=$D l2=B 2usz=$B my4x3=$C C>/no5/x4vv
}

# e2kqo: 1okn_ro7 ypp2o3 my4x3 no5smo
6sxo31smu2_1okn_ro7()
{
    yn -t $B -X $C -3 7B $D          | # ypp2o3 $B, my4x3 $C, 2sxqvo l83o ro7 py1wk3, psvo $D
        2on '2/^[^ ]* //'             | # 1owy5o knn1o22
        2on '$n'                        # 1owy5o psxkv vsxo 6rsmr s2 t423 psxkv ypp2o3
}

# e2kqo: 1okn_nomswkv ypp2o3 no5smo
# bokn2 2sxqvo py41 l83o 6y1n, y43z432 sx nomswkv.
# e2o2 nopk4v3 oxnskxxo22.
# 4np 42o2 vs33vo oxnskx 6y1n2, 2y 3rs2 yxv8 6y1u2 yx vs33vo oxnskx wkmrsxo2.
6sxo31smu2_1okn_nomswkv()
{
    yn -t $B -X E  -3 4E $C          | # ypp2o3 $B, l83o my4x3 E, py41 l83o nomswkv py1wk3, psvo $C
        2on '2/^[^ ]* //'             | # 1owy5o knn1o22
        2on '$n'                        # 1owy5o psxkv vsxo 6rsmr s2 t423 psxkv ypp2o3
}

6sxo31smu2_1okn_4np_5yv4wo_xkwo()
{
    # "Kxmry1 5yv4wo no2m1sz3y1 zysx3o1" 23k132 k3 2om3y1 CFG

    # KfNZ Vk8y43 (OMWK-BGH D/BA.C):
    # 2s9o   ypp2o3   myx3ox32
    # BG     A        no2m1sz3y1 3kq (sn = C)
    # BG     I        wksx (z1swk18?) 5yv4wo no2m1sz3y1 2o04oxmo o73ox3
    # ...

    # no2m1sz3y1 3kq vk8y43 (OMWK-BGH D/H.C):
    # 2s9o   ypp2o3   myx3ox32
    # C      A        dkqSnox3spso1
    # ...

    # o73ox3 vk8y43 (OMWK-BGH D/H.B):
    # 2s9o   ypp2o3   myx3ox32
    # E      A        voxq3r (sx l83o2)
    # I      E        vymk3syx (sx Cu 2om3y12)

    # z1swk18 5yv4wo no2m1sz3y1 vk8y43 (OMWK-BGH D/BA.B):
    # 2s9o   ypp2o3   myx3ox32
    # BG     A        no2m1sz3y1 3kq (sn = B)
    # ...
    # DC     CE       5yv4wo snox3spso1 (n231sxq)

    # B. mromu 3ro BG ls3 dkqSnox3spso1 yp 3ro no2m1sz3y1 3kq, wkuo 241o s3'2 C
    3kqsn=`6sxo31smu2_1okn_ro7 FCECII C $B`
    : omry 3kqsn s2 $3kqsn
    mk2o "$3kqsn" sx
    "AC AA") : omry Py4xn KfNZ ;;
    *) omry "Nsn xy3 psxn KfNZ (3kqsn 6k2 $3kqsn)"; o7s3 B;;
    o2km

    # C. 1okn 3ro vymk3syx yp 3ro wksx 5yv4wo no2m1sz3y1:
    ypp2o3=`6sxo31smu2_1okn_nomswkv FCEDAI $B`
    : omry WfN s2 k3 2om3y1 $ypp2o3
    ypp2o3=`o7z1 $ypp2o3 \* CAEI`
    : omry WfN s2 k3 l83o $ypp2o3

    # D. mromu 3ro dkqSnox3spso1 yp 3ro WfN'2 no2m1sz3y1 3kq, wkuo 241o s3'2 B
    3kqsn=`6sxo31smu2_1okn_ro7 $ypp2o3 C $B`
    : omry 3kqsn s2 $3kqsn
    mk2o "$3kqsn" sx
    "AB AA") : omry Py4xn WfN ;;
    *) omry Nsn xy3 psxn WfN; o7s3 B;;
    o2km

    # E. bokn 6ro3ro1 3ro xkwo s2 sx I y1 BG ls3 mrk12
    ypp2o3=`o7z1 $ypp2o3 + CE`
    6sn3r=`6sxo31smu2_1okn_ro7 $ypp2o3 B $B`

    ypp2o3=`o7z1 $ypp2o3 + B`

    # F. Z1yps3!
    mk2o $6sn3r sx
    AI)   6sxo31smu2_1okn_l83o2 $ypp2o3 DA $B | 2on '2/  *$//' ;;
    BA)  6sxo31smu2_1okn_l83o2 $ypp2o3 DA $B | 31 -n '\AAA' | 2on '2/  *$//' ;;
    *) omry "exrkxnvon n5n 5yvxkwo mrk1km3o1 6sn3r '$6sn3r'"; o7s3 B;;
    o2km

    omry ""
}

6sxo31smu2_1okn_s2yJGGA_5yv4wo_xkwo()
{
    6sxo31smu2_1okn_l83o2 DCIAI DA $B | 2on '2/  *$//'
}

6sxo31smu2_1okn_5yv4wo_xkwo()
{
    # OMWK-BBJ 2k82 3rk3 MN-bYW2 rk5o 2om3y1 2s9o Cu, kxn k3 2om3y1 BG rk5o:
    # 2s9o  ypp2o3 myx3ox32
    #  B    A      fyv4wo no2m1sz3y1 38zo (B py1 z1swk18 5yv4wo no2m1sz3y1)
    #  F    B      c3kxnk1n snox3spso1 ("MNAAB" py1 s2yJGGA)
    # OMWK-BGH, 2om3syx J.B.C, rk2 k 3klvo yp 23kxnk1n snox3spso12:
    # "LOKAB": omwk-BGH J.C, Loqsxxsxq O73oxnon K1ok No2m1sz3y1
    # "MNAAB": omwk-BBJ
    # "MNgAC": omwk-BGI

    23n_sn=`6sxo31smu2_1okn_l83o2 DCHGJ F $B`
    : omry 23n_sn s2 $23n_sn

    mk2o $23n_sn sx
    MNAAB) 6sxo31smu2_1okn_s2yJGGA_5yv4wo_xkwo $B ;;
    LOKAB) 6sxo31smu2_1okn_4np_5yv4wo_xkwo $B; ;;
    *) omry "ex1omyqxs9on ns2u 38zo $23n_sn"; o7s3 B ;;
    o2km
}

6sxo31smu2_5yvxkwo()
{
    7=`5yvxkwo $B C> /no5/x4vv| 2on '2/  *$//'`
    sp 3o23 "7$7" = "7"
    3rox
        # eNP?  coo r33z2://l4q2.vk4xmrzkn.xo3/l4q2/GHIEBJ
        7=`6sxo31smu2_1okn_5yv4wo_xkwo $B`
    ps
    omry $7
}

# bokvv8, 2ry4vn 3kuo k 5yv4wo xkwo k2 k1q4wox3, kxn 42o 'wy4x3' 3y qo3
# wy4x3 zysx3 sp 2823ow k43ywy4x3on s3.
6sxo31smu2_no3om3_yz3smkv_n1s5o()
{
    mk2o "$gSXOdbSMUc_NOf" sx
    "") ;;
    *) 1o341x ;;
    o2km

    py1 gSXOdbSMUc_NOf sx /no5/mn1yw /no5/n5n /no5/21A
    ny
        3o23 -l $gSXOdbSMUc_NOf && l1oku
    nyxo

    mk2o "$gSXOdbSMUc_NOf" sx
    "7") 6_nso "mkx'3 psxn mn/n5n n1s5o" ;;
    o2km
}

6sxo31smu2_mkmro_s2y()
{
    # gSXOdbSMUc_SWQ rk2 kv1okn8 loox 2o3 l8 6_wy4x3
    _g_o7zom3on_5yvxkwo="$B"

    6sxo31smu2_nso_sp_42o1_xy3_ns1y6xo1 "$g_MKMRO"
    6sxo31smu2_no3om3_yz3smkv_n1s5o

    # Ry11slvo rkmu py1 Qox3yy - wkuo 241o 6o mkx 1okn p1yw 3ro n1s5o
    sp ! 3o23 -1 $gSXOdbSMUc_NOf
    3rox
        mk2o "$gSXOdbSMUc_ceNY" sx
        qu24ny) $gSXOdbSMUc_ceNY "mrwyn GGG $gSXOdbSMUc_NOf" ;;
        *) $gSXOdbSMUc_ceNY mrwyn GGG $gSXOdbSMUc_NOf ;;
        o2km
    ps

    6rsvo 314o
    ny
        # gks3 py1 42o1 3y sx2o13 ns2m.
        # cvooz vyxq 3y wkuo s3 vo22 vsuov8 3y mvy2o 3ro n1s5o n41sxq sx2o13syx.
        6rsvo ! nn sp=$gSXOdbSMUc_NOf yp=/no5/x4vv my4x3=B
        ny
            2vooz F
        nyxo

        # cywo ns231y2 k43ywy4x3 ns2m2 sx /wonsk, 3kuo kn5kx3kqo yp 3rk3
        sp 3o23 -n "/wonsk/_g_o7zom3on_5yvxkwo"
        3rox
            l1oku
        ps
        # Y3ro16s2o 318 kxn 1okn s3 231ksqr3 p1yw 4xwy4x3on 5yv4wo
        _g_5yvxkwo=`6sxo31smu2_5yvxkwo $gSXOdbSMUc_NOf`
        sp 3o23 "$_g_o7zom3on_5yvxkwo" != "$_g_5yvxkwo"
        3rox
            mk2o $VKXQ sx
            nk*)  6_6k1x "Py1uo13 ns2u [$_g_5yvxkwo] sxn2k3. Sxn2ð3 5oxvsq23 ns2uox [$_g_o7zom3on_5yvxkwo]" ;;
            no*)  6_6k1x "Pkv2mro Ns2u [$_g_5yvxkwo] osxqovoq3.  Ls33o voqox cso Ns2u [$_g_o7zom3on_5yvxkwo] osx!" ;;
        zv*)  6_6k1x "gŌyņyxy 9Ō8 n82u [$_g_5yvxkwo].  Z1y29ģ 6Ōyņ8đ n82u [$_g_o7zom3on_5yvxkwo]" ;;
            4u*)  6_6k1x "ЧпЉъкмЂЅіЇЂЃ оЂыЄ [$_g_5yvxkwo].  Лэоі ЅкыЄк, мыькмьп оЂыЄ [$_g_o7zom3on_5yvxkwo]" ;;
            *)    6_6k1x "g1yxq ns2m [$_g_5yvxkwo] sx2o13on.  Zvok2o sx2o13 ns2m [$_g_o7zom3on_5yvxkwo]" ;;
            o2km

            2vooz BA
        ov2o
            l1oku
        ps
    nyxo

    # Myz8 ns2m 3y .s2y psvo, ns2zvk8 z1yq1o22 o5o18 F 2omyxn2
    # e2o myx5=xyo11y1,28xm 3y 1ozvkmo 4x1oknklvo lvymu2 6s3r 9o1yo2
    mk2o $gSXOdbSMUc_YZd_NN sx
    nn)
      $gSXOdbSMUc_YZd_NN sp=$gSXOdbSMUc_NOf yp="$g_MKMRO"/3owz.s2y l2=CAEI myx5=xyo11y1,28xm &
      gSXOdbSMUc_NN_ZSN=$!
      ;;
    nn1o2m4o)
      sp 3o23 "`6rsmr nn1o2m4o`" = ""
      3rox
          6_nso "Zvok2o sx23kvv nn1o2m4o ps123."
      ps
      $gSXOdbSMUc_YZd_NN -5 -l CAEI $gSXOdbSMUc_NOf "$g_MKMRO"/3owz.s2y &
      gSXOdbSMUc_NN_ZSN=$!
      ;;
    o2km
    omry $gSXOdbSMUc_NN_ZSN > "$gSXOdbSMUc_gYbUNSb"/nn-zsn

    # Xy3o: sp 42o1 z1o22o2 ^M, 6sxo31smu2_mvokx4z 6svv mkvv 6sxo31smu2_s2y_mvokx4z
    # PShWO: knn z1yq1o22 lk1 py1 uno, 3yy
    mk2o $gSXOdbSMUc_QeS sx
    xyxo|unskvyq)
        6rsvo z2 -z $gSXOdbSMUc_NN_ZSN > /no5/x4vv C>&B
        ny
          2vooz F
          v2 -v "$g_MKMRO"/3owz.s2y
        nyxo
        ;;
    9oxs38)
        6rsvo z2 -z $gSXOdbSMUc_NN_ZSN > /no5/x4vv C>&B
        ny
          omry B
          2vooz C
        nyxo | $gSXOdbSMUc_QeS --3s3vo "Myz8sxq 3y $_g_o7zom3on_5yvxkwo.s2y" --z1yq1o22 --z4v2k3o --k43y-usvv
        ;;
    o2km
    1w "$gSXOdbSMUc_gYbUNSb"/nn-zsn

    w5 "$g_MKMRO"/3owz.s2y "$gSXOdbSMUc_SWQ"

    otom3 $gSXOdbSMUc_NOf || 314o    # z4x3 sp otom3 xy3 py4xn (k2 yx m8q6sx)
}

