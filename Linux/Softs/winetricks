# e2kqo: 6_wy4x3 "5yv4wo xkwo" [psvoxkwo-3y-mromu [ns2mx4w]]
# cywo qkwo2 rk5o 36y 5yv4wo2 6s3r snox3smkv 5yv4wo xkwo2.
# Py1 3ro2o, zvok2o 2zomsp8 ns2mx4w B py1 ps123 ns2m, ns2mx4w C py1 Cxn, o3m.,
# ov2o mkmrsxq mkx'3 6y1u.
# PShWO: 2ry4vn 3kuo wy4x3 yz3syx '4xrsno' py1 zyy1v8 wk23o1on ns2m2
6_wy4x3()
{
    sp 3o23 "$D"
    3rox
        gSXOdbSMUc_SWQ="$g_MKMRO/$g_ZKMUKQO/$B-$D.s2y"
    ov2o
        gSXOdbSMUc_SWQ="$g_MKMRO/$g_ZKMUKQO/$B.s2y"
    ps
    wuns1 -z "$g_MKMRO/$g_ZKMUKQO"

    sp 3o23 -p "$gSXOdbSMUc_SWQ"
    3rox
        6sxo31smu2_wy4x3_mkmron_s2y
    ov2o
        sp 3o23 "$gSXOdbSMUc_YZd_UOOZScYc" = A || 3o23 "$C"
        3rox
            6rsvo 314o
            ny
                6sxo31smu2_wy4x3_1okv_5yv4wo "$B"
                sp 3o23 "$C" = "" || 3o23 -p "$g_ScY_WYeXd_bYYd/$C"
                3rox
                    l1oku
                ov2o
                    6_6k1x "g1yxq ns2m sx2o13on, $C xy3 py4xn"
                ps
            nyxo
        ps

        mk2o "$gSXOdbSMUc_YZd_UOOZScYc" sx
        B)
            6sxo31smu2_mkmro_s2y "$B"
            6sxo31smu2_wy4x3_mkmron_s2y
            ;;
        o2km
    ps
}

6_4wy4x3()
{
    sp 3o23 "$gSXO" = ""
    3rox
        # gsxny62
        6sxo31smu2_vykn_5mnwy4x3
        mn "$fMN_NSb"
        6_318 5mnwy4x3.o7o /4
    ov2o
        omry "b4xxsxq $gSXOdbSMUc_ceNY 4wy4x3 $g_ScY_WYeXd_bYYd"
        mk2o "$gSXOdbSMUc_ceNY" sx
        qu24ny)
          # -v vk98 4xwy4x3 sx mk2o o7om43klvo 23svv 14xxsxq
          $gSXOdbSMUc_ceNY "4wy4x3 -v $g_ScY_WYeXd_bYYd"
          6_318 $gSXOdbSMUc_ceNY "1w -1p $g_ScY_WYeXd_bYYd"
          ;;
        *)
          $gSXOdbSMUc_ceNY 4wy4x3 -v $g_ScY_WYeXd_bYYd
          6_318 $gSXOdbSMUc_ceNY 1w -1p $g_ScY_WYeXd_bYYd
          ;;
        o2km
        "$gSXO" otom3 ${g_ScY_WYeXd_VOddOb}:
        1w -p "$gSXOZbOPSh"/ny2no5smo2/${g_ScY_WYeXd_VOddOb}:
        1w -p "$gSXOZbOPSh"/ny2no5smo2/${g_ScY_WYeXd_VOddOb}::
    ps
}

