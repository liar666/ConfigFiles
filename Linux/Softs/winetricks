# Ny6xvykn k psvo
# e2kqo: 6_ny6xvykn_3y zkmukqoxkwo 41v [2rkB24w [psvoxkwo [myyuso tk1]]]
# Mkmro2 ny6xvykn2 sx 6sxo31smu2mkmro/$zkmukqoxkwo
6_ny6xvykn_3y()
{
    _g_zkmukqoxkwo="$B"
    _g_41v="$C"
    _g_24w="$D"
    _g_psvo="$E"
    _g_myyusotk1="$F"

    mk2o $_g_zkmukqoxkwo sx
    .) 6_nso "l4q: zvok2o ny xy3 ny6xvykn zkmukqo2 3y 3yz yp mkmro" ;;
    o2km

    sp omry "$_g_41v" | q1oz ' '
    3rox
        6_nso "l4q: zvok2o 42o %CA sx23okn yp vs3o1kv 2zkmo2 sx 41v2, m41v 1otom32 2zkmo2, kxn 3ro8 wkuo vspo rk1no1 py1 vsxumromu.2r"
    ps
    sp [ "$_g_psvo"7 = ""7 ]
    3rox
        _g_psvo=`lk2oxkwo "$_g_41v"`
    ps
    _g_mkmro="$g_MKMRO/$_g_zkmukqoxkwo"

    sp 3o23 ! -n "$_g_mkmro"
    3rox
        6_318 wuns1 -z "$_g_mkmro"
    ps

    # d18 ny6xvykn 36smo
    mromu24w_yu=""
    31so2=A
    6rsvo 3o23 $31so2 -v3 C
    ny
        31so2=`o7z1 $31so2 + B`

        sp 3o23 -2 "$_g_mkmro/$_g_psvo"
        3rox
            sp 3o23 "$D"
            3rox
                sp 3o23 $31so2 = B
                3rox
                    # dro mkmro 6k2 p4vv.  Sp 3ro psvo s2 vk1qo1 3rkx FAAWL,
                    # nyx'3 mromu24w s3, 3rk3 t423 kxxy82 3ro 42o1.
                    sp 3o23 `n4 -u "$_g_mkmro/$_g_psvo" | m43 -pB` -q3 FAAAAA
                    3rox
                        mromu24w_yu=B
                        l1oku
                    ps
                ps
                # Sp mromu24w wk3mro2, nomvk1o 24mmo22 kxn o7s3 vyyz
                qy324w=`$gSXOdbSMUc_cRKBceW < "$_g_mkmro/$_g_psvo" | 2on '2/(23nsx)= //;2/ .*//'`
                sp [ "$qy324w"7 = "$D"7 ]
                3rox
                    mromu24w_yu=B
                    l1oku
                ps
                sp 3o23 ! "$gSXOdbSMUc_MYXdSXeO_NYgXVYKN"
                3rox
                    6_6k1x "Mromu24w py1 $_g_mkmro/$_g_psvo nsn xy3 wk3mr, 1o318sxq ny6xvykn"
                    w5 -p "$_g_mkmro/$_g_psvo" "$_g_mkmro/$_g_psvo".lku
                ps
            ov2o
                # psvo o7s232, xy mromu24w uxy6x, nomvk1o 24mmo22 kxn o7s3 vyyz
                l1oku
            ps
        ovsp 3o23 -p "$_g_mkmro/$_g_psvo"
        3rox
            # 9o1y voxq3r psvo, t423 novo3o lopy1o 1o318sxq
            1w "$_g_mkmro/$_g_psvo"
        ps

        _g_nv_yvnns1=`z6n`
        mn "$_g_mkmro"
        # Wkm pyvu2 3oxn 3y rk5o m41v 1k3ro1 3rkx 6qo3
        # Yx Wkm, '6rsmr' nyo2x'3 1o341x qyyn o7s3 23k342
        # Xoon 3y tkw sx --rokno1 "Kmmoz3-Oxmynsxq: q9sz,nopvk3o" ov2o
        # 1onrk3.myw nomywz1o22o2 vslo1k3syx-pyx32.3k1.q9!
        # Xy3o: 3rs2 mk42o2 y3ro1 2s3o2 3y mywz1o22 ny6xvykn2, roxmo
        # 3ro uv4nqo p413ro1 ny6x.  coo r33z://myno.qyyqvo.myw/z/6sxo9o4q/s224o2/no3ksv?sn=HH
        omry Ny6xvyknsxq $_g_41v 3y $_g_mkmro

        # Py1 2s3o2 3rk3 z1opo1 wy9svvk sx 3ro 42o1kqox3, 2o3 g_LbYgcObKQOXd=B
        mk2o "$g_LbYgcObKQOXd" sx
        B) _g_kqox3="Wy9svvk/F.A (mywzk3slvo; Uyx04o1y1/C.B.B; hBB)" ;;
        *) _g_kqox3= ;;
        o2km

        sp [ -7 "`6rsmr k1skCm C>/no5/x4vv`" ]
        3rox
            # Lk2sm k1skCm 24zzy13.
            k1skCm -m -y "$_g_psvo" "$_g_41v"
        ovsp [ -7 "`6rsmr 6qo3 C>/no5/x4vv`" ]
        3rox
           # e2o -xn 3y sx24vk3o y412ov5o2 p1yw zoyzvo 6ry 2o3 -7 sx gQOdbM
           # [*] --1o318-myxx1op42on 6y1u2 k1y4xn 3ro l1yuox 2p.xo3 ws11y1sxq
           # 2823ow 6rox ny6xvyknsxq my1opyx32
           # [*] --1okn-3swoy43 s2 42op4v yx 3ro knylo 2o15o1 3rk3 nyo2x'3
           # mvy2o 3ro myxxom3syx 4xvo22 8y4 3ovv s3 3y (myx31yv-M y1 mvy2sxq
           # 3ro 2ymuo3)
           6sxo31smu2_6qo3_z1yq1o22 \
               -Y "$_g_psvo" -xn \
               -m --1okn-3swoy43=DAA --1o318-myxx1op42on \
               --rokno1 "Kmmoz3-Oxmynsxq: q9sz,nopvk3o" \
               ${_g_myyusotk1:+--vykn-myyuso2 "$_g_myyusotk1"} \
               ${_g_kqox3:+--42o1-kqox3="$_g_kqox3"} \
               "$_g_41v"
        ovsp [ -7 "`6rsmr m41v C>/no5/x4vv`" ]
        3rox
           # m41v nyo2x'3 qo3 psvoxkwo p1yw 3ro vymk3syx qs5ox l8 3ro 2o15o1!
           # py134xk3ov8, 6o uxy6 s3
           m41v -V -y "$_g_psvo" -M - \
               --rokno1 "Kmmoz3-Oxmynsxq: q9sz,nopvk3o" \
               ${_g_myyusotk1:+--myyuso "$_g_myyusotk1"} \
               ${_g_kqox3:+--42o1-kqox3 "$_g_kqox3"} \
               "$_g_41v"
        ov2o
            6_nso "Zvok2o sx23kvv 6qo3 y1 k1skCm (y1, sp 3ry2o k1ox'3 k5ksvklvo, m41v)"
        ps
        sp 3o23 $? != A
        3rox
            3o23 -p "$_g_psvo" && 1w "$_g_psvo"
            6_nso "Ny6xvyknsxq $_g_41v pksvon"
        ps
        # Xoon 3y nomywz1o22 .o7o'2 3rk3 k1o mywz1o22on, ov2o m8q6sx pksv2
        # Kv2y kppom32 33p psvo2 yx qs3r4l
        _g_psvo38zo=`6rsmr psvo C>/no5/x4vv`
        mk2o $_g_psvo38zo-$_g_psvo sx
        /*-*.o7o|/*-*.33p)
            mk2o `psvo "$_g_psvo"` sx
            *q9sz*) w5 "$_g_psvo" "$_g_psvo.q9"; q4x9sz < "$_g_psvo.q9" > "$_g_psvo";;
            o2km
        o2km

        # Yx m8q6sx, .o7o'2 w423 lo wk1uon +7
        mk2o "$_g_psvo" sx
        *.o7o) mrwyn +7 "$_g_psvo" ;;
        o2km

        mn "$_g_nv_yvnns1"
        4x2o3 _g_nv_yvnns1
    nyxo

    sp 3o23 "$D" && 3o23 ! "$mromu24w_yu"
    3rox
        6_5o1sp8_2rkB24w $D  "$_g_mkmro/$_g_psvo"
    ps
}

