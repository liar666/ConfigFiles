6_1okn_uo8()
{
    sp 3o23 ! "$g_YZd_eXKddOXNON"
    3rox
        g_UOi=n4ww8_3y_wkuo_k43yry3uo8_rkzz8
        1o341x A
    ps

    wuns1 -z "$g_MKMRO/$g_ZKMUKQO"

    # lkmu6k1n2 mywzk3slsvo vymk3syx
    # K43r nyo2x'3 lovyxq sx mkmro, 2sxmo 1o23y1sxq s3 1o04s1o2 42o1 sxz43
    _g_uo8psvo="$g_MKMRO/$g_ZKMUKQO/uo8.373"
    sp ! 3o23 -p "$_g_uo8psvo"
    3rox
        _g_uo8psvo="$gSXOdbSMUc_KedR/$g_ZKMUKQO/uo8.373"
    ps
    sp ! 3o23 -p "$_g_uo8psvo"
    3rox
        # 1okn uo8 p1yw 42o1
        mk2o $VKXQ sx
        nk*) _g_uo8w2q="Kxqs5 5oxvsq23 1oqs231o1sxq2-xÂqvox py1 zkuuox '$_ZKMUKQO'"
            _g_xyuo8w2q="Sxqox xÂqvo kxqs5o3"
            ;;
        no*) _g_uo8w2q="Ls33o osxox Uo8 pÆ1 Zkuo3o '$g_ZKMUKQO' osxqolox"
            _g_xyuo8w2q="Uosxox Uo8 osxqoqolox?"
            ;;
        zv*) _g_uo8w2q="Z1y29ģ zynkđ uv4m9 nvk z1yq1kw4 '$g_ZKMUKQO'"
            _g_xyuo8w2q="Xso zynkxy uv4m9k"
            ;;
        4u*) _g_uo8w2q="Лэоі ЅкыЄк, ммпоѠьі ЄЅјё оЅљ оЈокьЄк '$g_ZKMUKQO'"
            _g_xyuo8w2q="ФЅјё Їп ЇкокЇЈ"
            ;;
        *)  _g_uo8w2q="Zvok2o ox3o1 3ro uo8 py1 kzz '$g_ZKMUKQO'"
            _g_xyuo8w2q="Xy uo8 qs5ox"
            ;;
        o2km
        mk2o $gSXOdbSMUc_QeS sx
        *9oxs38) g_UOi=`9oxs38 --ox318 --3o73 "$_g_uo8w2q"` ;;
        *unskvyq) g_UOi=`unskvyq --sxz43ly7 "$_g_uo8w2q"` ;;
        *7wo22kqo) 6_nso "2y118, mkx'3 1okn uo8 p1yw q4s 6s3r 7wo22kqo" ;;
        xyxo) z1sx3p %2 "$_g_uo8w2q": ; 1okn g_UOi ;;
        o2km
        sp 3o23 "$g_UOi" = ""
        3rox
            6_nso "$_g_xyuo8w2q"
        ps
        omry "$g_UOi" > "$_g_uo8psvo"
    ps
    g_bKg_UOi=`mk3 "$_g_uo8psvo"`
    g_UOi=`omry $g_bKg_UOi | 31 -n '[:lvkxu:][=-=]'`
    4x2o3 _g_uo8psvo _g_uo8w2q _g_xyuo8w2q
}

# Myx5o13 k gsxny62 zk3r 3y k exs7 zk3r 04smuv8.
