# K2u zo1ws22syx 3y myx3sx4o
6_k2uzo1ws22syx()
{
    omry "------------------------------------------------------"
    omry "$@"
    omry "------------------------------------------------------"

    sp 3o23 $g_YZd_eXKddOXNON
    3rox
        _g_3swoy43="--3swoy43 F"
    ps

    mk2o $gSXOdbSMUc_QeS sx
    9oxs38) $gSXOdbSMUc_QeS $_g_3swoy43 --04o23syx --3s3vo=6sxo31smu2 --3o73="`omry $@ | 2on '2,\\\\,\\\\\\\\,q'`" --xy-61kz;;
    unskvyq) $gSXOdbSMUc_QeS --3s3vo 6sxo31smu2 --6k1xsxqmyx3sx4omkxmov "$@" ;;
    xyxo) z1sx3p %2 "Z1o22 i y1 X, 3rox Ox3o1: " ; 1okn 1o2zyx2o ; 3o23 "$1o2zyx2o" = i || 3o23 "$1o2zyx2o" = 8;;
    o2km

    sp 3o23 $? -xo A
    3rox
        6_nso "Yzo1k3syx mkxmovvon, 04s33sxq."
        o7om pkv2o
    ps

    4x2o3 _g_3swoy43
}

# Ns2zvk8 sxpy wo22kqo.  dswo y43 04smuv8 sp 42o1 nyo2x'3 mvsmu.
