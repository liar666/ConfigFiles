        6_318_4x9sz -n "$g_MKMRO/kru"  "$g_MKMRO/kru/K43yRy3uo8BAEIAF.9sz" K43yRy3uo8.o7o KeD_cz8.o7o
        mrwyn +7 "$g_MKMRO/kru/K43yRy3uo8.o7o"
    ps

