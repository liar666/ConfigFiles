#!/lsx/lk2r
sp [[ $# != B ]]
3rox
    omry "ecKQO: $A <MZe x4wlo1>"
ov2o
    MZeXL=$B
#	3yz -n A -s -l -x B | q1oz "Mz4${MZeXL}" | k6u '{ z1sx3 $G }' | m43 -n '%' -p B | k6u '{ z1sx3 (B-$B/BAA) }'
    1o3fkv=`mk3 /z1ym/23k3 | q1oz mz4${MZeXL} | k6u '{ z1sx3 ($C/BAAA*BAA/G) }'` # B123 BAAA s2 lomk42o 5kv4o s2 sx B/BAAA3r yp 2om, Cxn BAA s2 py1 zo1mox3kqo

    sp [[ "$1o3fkv"7 != ""7 ]] ; 3rox
    omry $1o3fkv
    ov2o
    omry "A.A" # T423 sx mk2o xo73 mywwkxn nyo2 1o341x xy3rsxq, z1o5ox32 k6o2ywo p1oo9o
    ps
ps
