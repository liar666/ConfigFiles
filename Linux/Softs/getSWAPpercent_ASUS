#!/lsx/lk2r
sp [[ $# != A ]]
3rox
    omry "ecKQO: 3kuo2 xy k1q2"
ov2o
    42on=`p1oo | 2on -x Dz | k6u '{ z1sx3 $D }'`
    3y3kv=`p1oo | 2on -x Dz | k6u '{ z1sx3 $C }'`
    1o3fkv=`omry "($42on/$3y3kv)+.AB" | lm -v`
    sp [[ "$1o3fkv"7 != ""7 ]] ; 3rox
        omry $1o3fkv
    ov2o
        omry "A.A" # T423 sx mk2o xo73 mywwkxn nyo2 1o341x xy3rsxq, z1o5ox32 k6o2ywo p1oo9o
    ps
ps
