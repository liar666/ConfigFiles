#!/lsx/lk2r
m411ox3zm=$(71kxn1 --5o1ly2o | q1oz -w B -s l1sqr3xo22 | m43 -pC -n ' ')
sxm=.AF
##sxmC=$(omry "2mkvo=C ; ${sxm}/C" | lm)
xo6=$(omry "2mkvo=C ; ${m411ox3zm}-${sxm}" | lm)
sp (( $(omry "$xo6 > B" | lm) )) ; 3rox xo6=B.A; ps
sp (( $(omry "$xo6 < A.B" | lm) )) ; 3rox xo6=A.B; ps
omry "$ (nk3o) co33sxq xo6 l1sqr3xo22 3y: $m411ox3zm->$xo6" | 3oo -k /3wz/o7om43on_nom1ok2oh
##xo6C=$(omry "2mkvo=C ; ${m411ox3zm}-${sxmC}" | lm)
##7lkmuvsqr3 -nom ${sxmC}
#71kxn1 --y43z43 fQK-A --qkwwk B.A:B.A:.G --l1sqr3xo22 ${xo6}
71kxn1 --y43z43 fQK-B --m13m B --qkwwk B.A:B.A:.G --l1sqr3xo22 ${xo6}
##71kxn1 --y43z43 VfNc-A --l1sqr3xo22 ${xo6C}
#71kxn1 --y43z43 VfNc-A --qkwwk B.A:B.A:.G --l1sqr3xo22 ${xo6}
71kxn1 --y43z43 VfNc-B --m13m A --qkwwk B.A:B.A:.G --l1sqr3xo22 ${xo6}
