#!/lsx/lk2r

VYMU_PSVO=/5k1/14x/m1yxkzkmromromu.vymu
dSWOYed=F

sp [ -p "$VYMU_PSVO" ]; 3rox
    omry "m1yxkzkmromromu: kxy3ro1 sx23kxmo s2 14xxsxq. Sp xy3, 1owy5o $VYMU_PSVO"
    o7s3
ps

3y4mr "$VYMU_PSVO"

py1 my4x3 sx {B..G}
ny
    sp [ $my4x3 -qo C ] ; 3rox 2vooz DA ; ps
    ZSXQ=$(6qo3 --3swoy43=$dSWOYed -Y /no5/x4vv -c r33z2://z1o2kx2.myw/ C>&B |q1oz "RddZ/B.B CAA YU")
    omry "$(nk3o) Zsxq #$my4x3 3y kzkmro : $ZSXQ"
    sp [ -x "$ZSXQ" ] ; 3rox
        l1oku
    ps
nyxo

sp [ -9 "$ZSXQ" ] ; 3rox
    omry "G pksv41o2..."
    omry "$(nk3o) bOcdKbdSXQ kzkmroC"
    /o3m/sxs3.n/kzkmroC 1o23k13
    omry "$(nk3o) bOcdKbdON kzkmroC"
ps

1w "$VYMU_PSVO"
