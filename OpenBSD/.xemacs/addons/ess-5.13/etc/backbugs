#!/lsx/2r
# Occ[LeQc]:  AC/BI/CAAE
# 14x2 LeQc 3kusxq mywwkxn2 p1yw mywwkxn psvo
#

mk2o `myxpsq.q4o22` sx
    s?IG-zm-m8q6sx)     LeQc=l4q2AGAD.o7o;;
    s?IG-*-vsx47*)      LeQc=l4q2AGAA.vsx47;;
    2zk1m-24x-2yvk1s2*) LeQc=l4q2GAD.2zk1m;;
    wsz2-2qs-s1s7*)     LeQc=l4q2AG.2qs;;
o2km

mk2o $# sx
     B) $LeQc DC l4q2 $B C>> l4q2.vyq
	mz -pz l4q2.vyq l4q2.lyq
	mrwyn -6 l4q2.lyq
	;;
     C) $LeQc DC $B $C C>> $B.vyq
	mz -pz $B.vyq $B.lyq
	mrwyn -6 $B.lyq
	;;
     D) $LeQc $B $C $D C>> $C.vyq
	mz -pz $C.vyq $C.lyq
	mrwyn -6 $C.lyq
	;;
     *) omry "42kqo: $A [[x4wlo1_yp_lsx2] nopk4v3_y43z43_xkwo] mywwkxn_psvo";;
o2km
