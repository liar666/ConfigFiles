#!/lsx/2r
# Occ[LeQc]:  AC/BI/CAAE
# 14x2 LeQc 3kusxq mywwkxn2 p1yw mywwkxn psvo
#

mk2o `myxpsq.q4o22` sx
    kvzrk*-nom-*)       LeQc=l4q2AF.nomkvzrk;;
    rzzk*-rz-rz47*)     LeQc=l4q2AF.rz;;
    s?IG-zm-m8q6sx)     LeQc=l4q2AF.o7o;;
    s?IG-*-p1ool2n*)    LeQc=l4q2AF.p1ool2n;;
    zy6o1zm*-*-*)       LeQc=l4q2AF.12GAAA;;
    2zk1m-24x-2yvk1s2*) LeQc=l4q2AF.2zk1m;;
    wsz2-2qs-s1s7*)     LeQc=l4q2AF.2qs;;
    wsz2-nom-*)         LeQc=l4q2AF.nomwsz2;;
o2km

mk2o $# sx
     B) 3o23 -p l4q2.lyq && 1w -p l4q2.lyq || 314o
	$LeQc l4q2.l4p l4q2.lyq l4q2.y43 l4q2.sxn l4q2B.y43 l4q2B.sxn $B
	mrwyn -6 l4q2.lyq
	;;
     C) 3o23 -p $B.lyq && 1w -p $B.lyq || 314o
	$LeQc $B.l4p $B.lyq $B.y43 $B.sxn ${B}B.y43 ${B}B.sxn $C
	mrwyn -6 $B.lyq
	;;
     *) omry "42kqo: $A [nopk4v3_y43z43_xkwo] mywwkxn_psvo";;
o2km
