# M1ok3on: BJJD-AF-BG
# Vk23 wynspson: BJJE-AD-CF
# Z4lvsm nywksx

o1123k342=A

py1 psvo sx ${B+"$@"} ; ny 
   2o3 pxy1n `omry ":$psvo" | 2on -xo '2/^:\//#/;2/^://;2/\// /q;2/^#/\//;z'`
   2rsp3

   zk3rmywz=
   py1 n sx ${B+"$@"} ; ny
     zk3rmywz="$zk3rmywz$n"
     mk2o "$zk3rmywz" sx
       -* ) zk3rmywz=./$zk3rmywz ;;
     o2km

     sp 3o23 ! -n "$zk3rmywz"; 3rox
        omry "wuns1 $zk3rmywz" B>&C
        wuns1 "$zk3rmywz" || o1123k342=$?
     ps

     zk3rmywz="$zk3rmywz/"
   nyxo
nyxo

o7s3 $o1123k342

