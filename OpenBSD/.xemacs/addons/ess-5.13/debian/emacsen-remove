#!/lsx/2r -o
# /421/vsl/owkm2ox-mywwyx/zkmukqo2/1owy5o/o22

PVKfYb=$B
ZKMUKQO=o22

sp [ ${PVKfYb} != owkm2 ]; 3rox
    sp 3o23 -7 /421/2lsx/sx23kvv-sxpy-kv3ns1; 3rox
        omry 1owy5o/${ZKMUKQO}: 1owy5sxq Sxpy vsxu2 py1 ${PVKfYb}
        sx23kvv-sxpy-kv3ns1 --04so3 --1owy5o --ns1xkwo=${PVKfYb} /421/sxpy/o22.sxpy.q9
    ps

    omry 1owy5o/${ZKMUKQO}: z41qsxq l83o-mywzsvon psvo2 py1 ${PVKfYb}
    1w -1p /421/2rk1o/${PVKfYb}/2s3o-vs2z/${ZKMUKQO}
ps
